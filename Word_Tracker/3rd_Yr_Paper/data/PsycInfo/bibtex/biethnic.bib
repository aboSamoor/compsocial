@article{2012-15968-00920120601,
Abstract = {This article presents a study which aims to compare the barriers to physical activity reported by persons with diabetes (PwD) and those without diabetes in a large, rural, biethnic population-based sample. Using 5-point Likert scales, seven barriers to physical activity were assessed as the extent to which participants would increase their physical activity if the barrier were absent. Using t tests, chi-square, and ANOVA procedures, it was found that PwD walked less and were less active than those without diabetes even when controlling for age, sex, BMI, and ethnicity. While this study was limited by the relatively small number of PwD in the sample and reliance on self-report measures of behavior and clinical diagnosis, the use of population-based recruitment in a rural, biethnic sample are strengths. Further, the findings provide direction for counseling and messaging around physical activity for PwD. Specifically, diabetes care providers should ask patients about their fear of injury so that these fears can be addressed when providing physical activity recommendations. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
ISSN = {0895-2779, 1543-2904},
Journal = {Journal of Sport & Exercise Psychology},
Keywords = {physical activity, diabetes, clinical diagnosis, rural population, Diabetes, Diagnosis, Physical Activity, Rural Environments},
Number = {3},
Pages = {431 - 432},
Title = {If I weren't afraid, I'd do more: Fear as a barrier to physical activity in persons with diabetes.},
