Number = {1},
Pages = {56 - 66},
Title = {Correlates for psychological, physical, emotional and ritualistic forms of child abuse among high school students in the Northern Province, South Africa.},
Volume = {11},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2000-15548-005&site=ehost-live&scope=site},
