Author = {Mo, Frank and  Choi, Bernard C. K. and  Morrison, Howard and  Merrick, Joav and  Merrick, Joav (Ed)},
Booktitle = {Public health yearbook, 2011.},
ISBN = {2164-716X, 978-1-62081-433-8, 978-1-62081-434-5},
Pages = {375 - 386},
Publisher = {Nova Biomedical Books},
