Journal = {Acta Psychiatrica Scandinavica},
Keywords = {cognition, homeless persons, mental disorders, neuropsychology, Cognitive Impairment, Homeless, Mental Disorders, Neurocognition},
Number = {4},
Pages = {256 - 268},
Title = {Neurocognitive impairment in a large sample of homeless adults with mental illness.},
URL = {stergiopoulosv@smh.ca},
Volume = {131},
Year = {2015},
}


@article{1999-03091-00619990701,
Abstract = {Compared the incidence, nature and long-term outcome of psychosis in different ethnic groups in the UK. A 5-yr, prospective study of an epidemiological cohort of people with a first contact for psychosis was conducted. 39 Whites, 38 Blacks, 11 Asians, and 5 patients described as Other were recruited. The patients were interviewed using a semistructured questionnaire that covered demographic details and a conventional history. Psychiatric symptoms were assessed using the 9th edition of the Present State Examination and the Syndrome Checklist. Age-standardized incidence rates for schizophrenia and non-affective psychosis were higher for Black and Asian people than Whites. Stability of diagnosis and course of illness were similar in all ethnic groups. During the 5th yr, Black people were more likely than others to be detained, brought to hospital by the police and given emergency injections. Illness symptoms, social factors (admissions, compulsory detention, police contact), and treatment were also examined. The results suggest that the nature and outcome of psychotic illness is similar in all ethnic groups but Black people experience more adverse contacts with services later in the course of illness. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Goater, Nicky and  King, Michael and  Cole, Eleanor and  Leavey, Gerard and  Johnson-Sabine, Eric and  Blizard, Robert and  Hoar, Amanda},
