@article{1993-40402-00119930501,
Abstract = {Two experiments investigated whether the orientation-specific right hemisphere advantage (RHA) for face recognition (FR) depends on lateralized configural coding mechanisms, and whether the use of such mechanisms requires expertise. In Exp 1, 32 Chinese residents of Singapore and 32 Caucasian residents of New Zealand were presented photographs of Chinese and Caucasian faces. Ss showed expertise-related use of configural coding, but no orientation-specific RHA advantage. In Exp 2, 64 Caucasian residents of New Zealand were shown photographs bilaterally, unilaterally, or chimerically. Ss with high hemispheric arousal asymmetry showed an orientation-specific RHA advantage, but no expertise-related use of configural coding. Low arousal asymmetry Ss showed the opposite pattern. Results suggest that different kinds of configural coding may underlie ethnicity-related expertise effects and laterality effects in FR. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Rhodes, Gillian},
ISSN = {0278-2626},
Journal = {Brain and Cognition},
