Volume = {16},
Year = {2002},
}


@article{2000-15541-00620000301,
Abstract = {Investigated the extent to which social work students consider cultural awareness and competency as being relevant to their professional practice. 75 undergraduate and graduate college students (average age 30.5 yrs) enrolled in social work practice courses completed surveys. Results show that 65% of Ss did not mention cultural awareness or competency in response to any questions. No Ss mentioned cultural competency when asked to define the role of the social worker in the health care system, or when asked to list changes needed to make the social service system more responsive to clients. Only 13% of Ss included cultural content in response to questions concerning the most important characteristics of social workers. No differences were found between White Ss and those of other ethnicity. It is concluded that multicultural awareness is significantly lacking in social work students' education and practice. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
