@article{2014-14800-01620140501,
Abstract = {Introduction: Physical inactivity is a risk factor for cancer morbidity and mortality, but its influence in colorectal cancer (CRC) survivors is understudied. We investigated sociodemographic, physically limiting, and behavioral predictors influencing leisure time physical activity (LTPA) among CRC survivors. Methods: Pooled 1997–2010 National Health Interview Survey data (N = 2378) were used to evaluate LTPA compliance in CRC survivors according to Healthy People 2010 recommendations. Univariate and multivariable logistic regression analyses were performed to identify predictors of LTPA compliance among CRC survivors. Independent variables included: age, gender, race/ethnicity, education, health insurance, body mass index (BMI), ≥2 chronic conditions limiting physical activity, time since cancer diagnosis, and poverty, marital, smoking and alcohol status. Results: Multivariable regression models reveal that Hispanics, non-Hispanic Blacks, those with ≥2 physically limiting chronic conditions, and current smokers were less likely to comply with LTPA recommendations. CRC survivors who were of “other” race, more than one race, those with some college degree or college degree, and current drinkers were more likely to comply. Discussion: Hispanics, non-Hispanic Blacks, those with >2 physically limiting chronic conditions and current smokers warrant additional efforts to encourage physical activity and to determine the impact of regular physical activity on CRC survivorship. (PsycINFO Database Record (c) 2015 APA, all rights reserved). (journal abstract)},
Author = {D'Andrea, Anthony P. and  Fernandez, Cristina A. and  Tannenbaum, Stacey L. and  Clarke, Tainya C. and  McClure, Laura A. and  LeBlanc, William G. and  Lee, David J.},
ISSN = {0091-7435},
Journal = {Preventive Medicine: An International Journal Devoted to Practice and Theory},
Keywords = {Cancer survivor, Colorectal cancer, Physical activity, Compliance, Leisure Time, Neoplasms, Physical Activity, Survivors},
Pages = {78 - 82},
Title = {Correlates of leisure time physical activity compliance in colorectal cancer survivors.},
URL = {apdandrea@med.miami.edu, cfernandez5@med.miami.edu, STannenbaum@med.miami.edu, tclarke2@med.miami.edu, lmcclure@med.miami.edu, thedatadoctor@gmail.com, dlee@med.miami.edu},
Volume = {62},
