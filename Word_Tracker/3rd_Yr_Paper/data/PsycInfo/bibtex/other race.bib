Number = {4},
Pages = {484 - 495},
Title = {Brief daily exposures to Asian females reverses perceptual narrowing for Asian faces in Caucasian infants.},
URL = {g.anzures@bbk.ac.uk, kang.lee@utoronto.ca},
Volume = {112},
Year = {2012},
}


@article{2012-03454-00120120601,
Abstract = {Purpose: To describe uncorrected distance visual impairment (VI). Methods: We conducted an analysis of the 3,555 adolescents aged 12 through 21 years who participated in the 2005–2008 National Health and Nutrition Examination Survey. Distance VI was defined as 20/40 or worse in the better-seeing eye. Data were weighted to represent the civilian noninstitutionalized population. Results: Overall, 12.3% (95% confidence interval [CI]: 10.7%–14.1%) had distance VI, which was correctable to 20/30 or better in both eyes in 86.1% (95% CI: 83.6%–89.5%). The prevalence was higher among those who reported not having corrective lenses available (44.3%) compared with those who reported that they did not need them (8.5%) or who had them available (5.2%; p < .001). After adjusting for potential confounders, those who were 12 or 13 years of age had 2.27 (95% CI: 1.32–3.90) greater odds of distance VI than older adolescents, and the odds of distance VI were greater among non-Hispanic blacks (1.66 [95% CI: 1.11–2.48]), Hispanics (1.96 [95% CI: 1.35–2.84]), or other race/ethnicities (2.06 [95% CI: 1.19–3.57]) than among non-Hispanic whites. Conclusions: More than 1 in 10 adolescents had uncorrected distance VI. To address this, interventions should address case detection, access to eye care, and adherence with corrective lenses. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Kemper, Alex R. and  Wallace, David K. and  Zhang, Xinzhi and  Patel, Nita and  Crews, John E.},
ISSN = {1054-139X},
Journal = {Journal of Adolescent Health},
Keywords = {uncorrected distance visual impairment, adolescents, prevalence, United States, Epidemiology, Vision Disorders, Distance Perception},
Number = {6},
Pages = {645 - 647},
Title = {Uncorrected distance visual impairment among adolescents in the United States.},
URL = {alex.kemper@duke.edu},
Volume = {50},
Year = {2012},
}


@article{2014-13022-01020140401,
Abstract = {Reviews the article, Extreme Binge Drinking Among 12th-Grade Students in the United States by M. E. Patrick, J. E. Schulenberg, and M. E. Martz (2013). The risks associated with alcohol use among youth in the United States has been previously reported. The authors of the current study examined binge drinking and extreme binge drinking among a national sample of US high school seniors between 2005 and 2011. More than 16,000 (52.3% female) students participated. Two thirds of participants were white, 11% black, 13.1% Hispanic, and 11.5% of other race/ethnicity. The authors discuss that while youth binge drinking has decreased since record-high levels in the late 1970s, extreme binge drinking does not seem to be declining. One significant limitation of this study is that students who dropped out of high school before their senior year are not included. Therefore, it is possible that the true frequency of binge and extreme binge drinking among youth of this age group may be even higher than indicated by these results. The data presented in this study could be used in development of prevention and intervention programs aimed at youth in the United States. (PsycINFO Database Record (c) 2014 APA, all rights reserved)},
