Journal = {Journal of Personality and Social Psychology},
Keywords = {extent & similarity of physical & psychological distinctions in judging individual members of own vs other races, White vs Black vs Korean college students, US & Korea, Face Perception, Racial and Ethnic Attitudes, Racial and Ethnic Differences, Asians, Blacks, Stereotyped Attitudes, Whites},
Number = {1},
Pages = {85 - 101},
Title = {They don't all look alike: Individual impressions of other racial groups.},
Volume = {65},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1993-41046-001&site=ehost-live&scope=site},
Year = {1993},
