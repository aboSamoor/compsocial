Number = {3},
Pages = {288 - 303},
Title = {Perceptions of interracial couples: Prejudice at the dyadic level.},
URL = {jackso67@msu.edu},
Volume = {27},
Year = {2001},
}


@article{1993-13244-00119920101,
Abstract = {Examined whether race and gender are related to discontent as indicated by the anomia of despair and distrust and of job and financial dissatisfaction during 9 yrs of General Social Survey data (1973–1987). Blacks' discontent exceeded Whites' in most instances; Black women more frequently showed significantly higher discontent than other race/gender groups. Also, Blacks' discontent more often exceeded Whites' among persons high in SES than among those low in SES. Gender main effects were almost nonexistent; White women almost never showed more discontent than other race/gender groups, attributable perhaps to White women's greater identification with race than gender and their perception of general but not personal discrimination. As expected, racial differences in discontent tended to be higher during the Nixon-Ford and Reagan US presidential administrations than during the Carter administration. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
