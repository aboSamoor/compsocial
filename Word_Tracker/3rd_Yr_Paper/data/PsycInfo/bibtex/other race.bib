Number = {1},
Pages = {17 - 32},
Title = {Unemployment, drug use, and HIV risk among American Indian and Alaska Native drug users.},
Volume = {9},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2001-03442-003&site=ehost-live&scope=site},
Year = {2000},
}


