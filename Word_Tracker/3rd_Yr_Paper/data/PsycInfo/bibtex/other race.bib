Keywords = {health care careers, hospital, office administration, race, ethnicity, physician, workplace discrimination, job turnover, career satisfaction, career change, Employee Turnover, Employment Discrimination, Health Care Services, Job Satisfaction, Physicians, Ethnic Identity, Race and Ethnic Discrimination},
Number = {12},
Pages = {1274 - 1282},
Title = {Health care workplace discrimination and physician turnover.},
