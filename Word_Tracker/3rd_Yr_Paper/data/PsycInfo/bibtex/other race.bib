ISSN = {0031-5125, 1558-688X},
Journal = {Perceptual and Motor Skills},
Keywords = {functional race membership, recognition of faces, black & white college students, Blacks, Race (Anthropological), Recognition (Learning), Social Perception, Whites, Membership},
Number = {2},
Pages = {455 - 462},
Title = {'Functional race membership' and recognition of faces.},
