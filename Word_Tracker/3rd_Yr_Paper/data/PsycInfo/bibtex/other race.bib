Number = {7},
Pages = {406 - 410},
Title = {A comparison of partner notification effectiveness in African-, Caribbean-, and United States-born HIV-infected Blacks in New York City.},
URL = {atsega@health.nyc.gov},
Volume = {26},
Year = {2012},
}


