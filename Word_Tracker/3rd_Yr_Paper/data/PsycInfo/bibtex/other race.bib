}


@article{1987-21002-00119860401,
Abstract = {Conducted 3 experiments with 61 college students and 20 nonstudents to test the view that recognition of faces is more impaired by inversion than recognition of other objects normally only seen upright, as proposed by R. K. Yin (see record [rid]1969-12269-001[/rid]). Exp I explored the possibility that this result is explicable in terms of the familiarity of the recognition tasks rather than a face-specific factor. Ss were shown slides of 32 Black or White faces, with 16 target and 16 distractor items. Results show that a less familiar task (recognizing other race faces) was more disrupted by inversion than recognizing own race faces. In Exp II, Yin's finding was replicated using a different view of items at test. In Exp III, Ss who were encouraged to make personality judgments on initial viewing of the faces were no more impaired by inversion at test than Ss encouraged to name distinctive physical features. Results imply that the disproportionate effect of inversion upon face recognition cannot be explained in terms of the extra familiarity of the task or the use of identical photographs at test. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Valentine, Tim and  Bruce, Vicki},
ISSN = {0001-6918},
Journal = {Acta Psychologica},
Keywords = {race & familiarity & inversion & personality judgment, face recognition, college students & nonstudents, Face Perception, Familiarity, Human Information Storage, Racial and Ethnic Differences, Spatial Organization, Attribution, Memory},
Number = {3},
Pages = {259 - 273},
Title = {The effect of race, inversion and encoding activity upon face recognition.},
Volume = {61},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1987-21002-001&site=ehost-live&scope=site},
Year = {1986},
}


@article{1986-11646-00119851201,
Abstract = {Investigated the hypothesis that "own-race bias," the tendency of individuals to perceive more similarity in the appearance of other-race members than in their own, may affect lineup development in criminal investigation. 90 Black and 78 White undergraduates were asked to construct lineups using both Black and White "suspects." On 3 of 4 measures, their behavior was similar while making White lineups but different on Black lineups. Relative to their performance on White lineups, White Ss became less selective on Black lineups but Black Ss became even more selective on Black lineups. Both groups displayed own-race bias by being more selective about own-race photos than other-race photos. This lessening of selectivity may make lineups constructed by cross-race lineup constructors less fair than lineups constructed by own-race lineup constructors. (18 ref) (PsycINFO Database Record (c) 2014 APA, all rights reserved)},
Author = {Brigham, John C. and  Ready, David J.},
ISSN = {0147-7307, 1573-661X},
Journal = {Law and Human Behavior},
Keywords = {own-race bias in construction of lineups using Black & White 'suspects', Black vs White college students in mock criminal investigations, Blacks, Legal Processes, Racial and Ethnic Differences, Stereotyped Attitudes, Whites},
Number = {4},
