Year = {2014},
}


@article{2014-10461-00320140501,
Abstract = {The current study examined developmental differences from the age of 5 to 18 in the creation process of own- and other-race facial composites. In addition, it considered how differences in the creation process affect similarity ratings. Participants created two composites (one own- and one other-race) from memory. The complexity of the composite creation process was recorded during Phase One. In Phase Two, a separate group of participants rated the composites for similarity to the corresponding target face. Results support the cross-race effect, developmental differences (based on composite creators) in similarity ratings, and the importance of the creation process for own- and other-race facial composites. Together, these findings suggest that as children get older the process through which they create facial composites becomes more complex and their ability to create facial composites improves. Increased complexity resulted in higher rated composites. Results are discussed from a psycho-legal perspective. (PsycINFO Database Record (c) 2014 APA, all rights reserved). (journal abstract)},
Author = {Kehn, Andre and  Renken, Maggie D. and  Gray, Jennifer M. and  Nunez, Narina L.},
ISSN = {0022-3980, 1940-1019},
Journal = {The Journal of Psychology: Interdisciplinary and Applied},
Keywords = {developmental trends, age differences, own-race facial composites, other-race facial composites, ability level, Adolescent Development, Age Differences, Childhood Development, Face Perception, Trends, Ability Level, Racial and Ethnic Differences},
Number = {3},
Pages = {287 - 304},
Title = {Developmental trends in the process of constructing own- and other-race facial composites.},
URL = {andre.kehn@und.edu},
Volume = {148},
Year = {2014},
}


@article{2013-38381-01520130601,
Abstract = {Presents a study which aims to examine whether the other-race effect in face recognition is sensitive to face format at encoding. The results of the study demonstrated that the other-race effect in face recognition is sensitive to the face format at encoding. The equivalent performance in recognizing own-race and other-race faces suggests that face shown in motion or in multiviews may modulate the holistic processing engaged in own-race faces processing and/or improve the poor structural information of other-race faces. The categorization-individuation model has difficulty to readily account for the data, as there was no manipulation of motivation to categorize/individualize faces at encoding. The other-race effect appears to be more malleable and less robust than previously thought. (PsycINFO Database Record (c) 2014 APA, all rights reserved)},
Author = {Zhao, Mintao and  Bülthoff, Isabelle},
ISSN = {1350-6285, 1464-0716},
Journal = {Visual Cognition},
