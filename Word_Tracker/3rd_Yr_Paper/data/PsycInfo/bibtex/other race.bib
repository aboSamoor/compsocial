Journal = {Journal of General Internal Medicine},
Keywords = {medication non-adherence, mortality, race, ethnicity, veterans, diabetes, Death and Dying, Diabetes, Ethnic Identity, Race (Anthropological), Treatment Compliance, Military Veterans},
Number = {2},
Pages = {208 - 215},
Title = {Differential impact of longitudinal medication non-adherence on mortality by race/ethnicity among veterans with diabetes.},
