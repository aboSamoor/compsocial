Keywords = {other race effect, face recognition, childhood development, European Caucasians, Childhood Development, Face Perception, Korean Cultural Groups, Race (Anthropological), Early Experience},
Number = {6},
Pages = {440 - 444},
Title = {Reversibility of the Other-Race Effect in Face Recognition During Childhood.},
URL = {pallier@shfj.cea.fr, schonen@psycho.univ-paris5.fr},
Volume = {16},
Year = {2005},
}


@article{2012-28558-01520120501,
Abstract = {Reviews the book, The Nature of Race: How Scientists Think and Teach About Human Difference by Ann Morning (2011). The phrase "race is a social construction" has become a tired truism. The sociology of race commonly begins by asserting that race as biology or the "essentialist" perspective was prevalent in the early days of scholarship on the topic but has since given way to a "constructionist" viewpoint. We now "know" that race, while not a biological reality, is most certainly a social one. But beyond the walls of sociology classes and scholarship, how prevalent is a constructionist viewpoint? Ann Morning's The Nature of Race: What Scientists Think and Teach about Human Difference answers this call by providing a thoughtful, rigorous, and highly compelling treatise on the reach of race concepts and the notion that differences between races are "essential," thus grounded in biology or "constructed" and contingent upon political, social, and cultural realities. Drawing on a wide array of sources—from content analyses of high school textbooks, to surveys and in-depth interviews with 41 faculty (from the natural and social sciences) and 49 students from a variety of majors, Morning paints a rich and complex picture of a rarely studied facet of racial thinking: how people define it. The book also examines the status of race concepts outside of the classroom, in realms generally perceived to be race neutral—the U.S. census, the law, criminal forensics, and the medical community. The main claim is that biological essentialism is very much alive, but not well understood. Morning begins with a history of race concepts, including the rise of race science and ending with the demise of eugenics. This sets the stage for documenting the splintering of race concepts held by the public and those held by professional scientists. Morning builds an exhaustive case for the need to know how the public defines race, even as many scholars have researched how the public thinks about race relationships. The book reveals that essentialism is not only more complex than previously thought but also more prevalent among scientists and students than constructionism. Although this book provides a largely neutral treatment of two competing perspectives, in the closing chapter Morning considers the implication that we may be going down an essentialist route. She highlights the fact that the viewing racial difference as essential removes the need to examine the impact of any type of discrimination when interpreting the persistence of inequality. Well-written, well-researched, and well-argued, The Nature of Race does an excellent job of tracing out the power of knowing how race is defined, leaving room for future research to examine how race concepts emerge among a variety of university settings, student populations, and institutions, and how race definitions underlie other race-related phenomenon. While "race as a social construction" is a common phrase, scholars in the field cannot be the only ones who understand it. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Bratter, Jenifer},
ISSN = {0002-9602},
Journal = {American Journal of Sociology},
Keywords = {human differences, race, scientists, sociology, communities, racial differences, Communities, Race (Anthropological), Racial and Ethnic Differences, Sociology, Scientists},
Number = {6},
Pages = {1846 - 1848},
Title = {Review of The nature of race: How scientists think and teach about human difference.},
Volume = {117},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2012-28558-015&site=ehost-live&scope=site},
Year = {2012},
}


@article{2007-17552-00320071201,
Abstract = {This study demonstrates that experience and development interact to influence the "cross-race effect." In a multination study (n = 245), Caucasian children and adults of European ancestry living in the United States, Norway, or South Africa, as well as biracial (Caucasian-African American) children and adults living in the United States, were tested for recognition of Asian, African, and Caucasian faces. Regardless of national or biracial background, 8- to 10-year-olds, 12- to 14-year-olds, and adults recognized own-race faces more accurately than other-race faces, and did so to a similar extent, whereas 5- to 7-year-olds recognized all face types equally well. This same developmental pattern emerged for biracial children and adults. Thus, early meaningful exposure did not substantially alter the developmental trajectory. During young childhood, developmental influences on face processing operate on a system sufficiently plastic to preclude, under certain conditions, the cross-race effect. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Goodman, Gail S. and  Sayfan, Liat and  Lee, Jennifer S. and  Sandhei, Marianne and  Walle-Olsen, Anita and  Magnussen, Svein and  Pezdek, Kathy and  Arredondo, Patricia},
ISSN = {0022-0965},
Journal = {Journal of Experimental Child Psychology},
