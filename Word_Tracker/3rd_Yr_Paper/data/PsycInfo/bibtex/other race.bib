Volume = {20},
Year = {2007},
}


@article{2009-03599-00420090401,
Abstract = {Without quantitative data to confirm or refute assertions made from qualitative data, it is hard to judge the potential impact of intervention approaches to reduce individuals and sub-populations’ risk of HIV infection. In contrast to what may be common perception and some researchers assertions about the role of racial stereotypes and their effect on sexual risk taking, the quantitative evidence presented here suggests that Asian Pacific Islander (API) men who have sex with men (MSM) are at no particularly higher risk for unprotected receptive anal intercourse (URAI) with White partners. Furthermore, API MSM appear to be able to use condoms with White and other race partners at rates similar to other groups of MSM. We concur that in-depth research is needed. Qualitative and quantitative investigations must work together to enhance our ability to clearly define the factors that contribute to HIV risk taking. More robust inquiries, including both methodological approaches, are needed to develop innovative HIV prevention interventions that address the complex underlying factors contributing to HIV risk taking among API MSM. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Raymond, H. Fisher and  McFarland, Willi},
