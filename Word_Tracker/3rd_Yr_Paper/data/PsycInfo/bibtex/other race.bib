Number = {8},
Pages = {968 - 973},
Title = {Provider barriers to hydroxyurea use in adults with sickle cell disease: A survey of the Sickle Cell Disease Adult Provider Network.},
URL = {slanzkr@jhmi.edu},
Volume = {100},
Year = {2008},
}


@article{2001-10054-00220020101,
Abstract = {Examines the temporal association between body image and depressive symptoms in an ethnically diverse sample of male and female adolescents. It was anticipated that body image would have a stronger impact on the depressive symptoms of girls than of boys and that African Americans would be less likely than the other groups to exhibit increased depressive symptomatology as a function of a change toward a more negative body image. Body image, depressive symptoms, and self esteem were each assessed at 2 points in time, approximately 13 mo apart. 877 African American, Asian American, White, Hispanic, or other ethnicity 12–17 yr olds were interviewed during Time 1, and 675 adolescents were reinterviewed for Time 2. Findings showed that girls were more influenced by change in body image than boys, but our assumption that African American girls would be least affected by body image changes was strongly disconfirmed. When the body image of African American teens declines, they experience a significant increase in psychological distress, and this increase is steeper than that observed in girls from other race/ethnicity groups. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
