Volume = {21},
Year = {2007},
}


@article{2007-05921-00520070401,
Abstract = {The familiar face overgeneralization hypothesis holds that an own-race positivity bias is, in part, a perceptual by-product of reactions to familiar people versus unfamiliar-looking strangers. Because prototypical facial structure varies across racial groups and communities are often racially segregated, strangers from one's own racial group should appear more familiar than strangers from a different racial group, contributing to ingroup favoritism and negative outgroup stereotypes. As predicted, the lower familiarity of own- than other-race faces mediated Koreans' and White Americans' ingroup favoritism in Study 1 and Black and White Americans' ingroup favoritism in Study 2. Lower familiarity of other-race faces also mediated negative stereotypes of other-race faces and partially suppressed positive ones, with familiarity effects confined to affectively valenced stereotypes. The results suggest that the unfamiliarity of other-race faces contributes not only to ingroup favoritism but also to a dual-process stereotyping in which both cultural beliefs and negative affective reactions to unfamiliarity make a contribution. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Zebrowitz, Leslie A. and  Bronstad, P. Matthew and  Lee, Hoon Koo},
ISSN = {0278-016X},
Journal = {Social Cognition},
Keywords = {face familiarity, ingroup favoritism, stereotyping, cultural beliefs, facial structure, White Americans, Black Americans, Koreans, Face Perception, Familiarity, Ingroup Outgroup, Stereotyped Attitudes, Blacks, Communities, Korean Cultural Groups, Racial and Ethnic Groups, Whites},
Number = {2},
Pages = {306 - 338},
Title = {The contribution of face familiarity to ingroup favoritism and stereotyping.},
URL = {zebrowitz@brandeis.edu},
Volume = {25},
Year = {2007},
}


@article{2007-03545-00320070201,
Abstract = {Background: Aim of this study was to verify the relationship between attachment and nomination of prosocial and aggressive classmates in Italian children attending ethnically mixed school classes. The hypothesis was that secure children nominate aggressive and prosocial peers independently by their ethnia, while insecure children nominate more own-race than other-race classmates for prosocial behaviour and nominate other-race classmates for aggressive behaviour. Method: Choices of prosocial and aggressive classmates were assessed by Peer Nomination Techniques (Coie, Dodge and Coppotelli, 1982). Attachment Internal Working Models were assessed by Attili modified version of the Separation Anxiety Test (2001). The sample was made up by 354 primary school youngsters: 252 Italian children (mean age 9.2 yrs) and 102 immigrant children (mean age 9.7 yrs). Results: Findings show Italian children with secure, ambivalent and avoidant attachment do not make any distinction among own-race and other-race classmates for prosocial and aggressive behaviour. Confused and disorganized italian children nominate more own-race classmates for prosocial behaviour, while nominate more other-race classmates for aggressive behaviour. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
