Volume = {34},
Year = {2008},
}


@article{2008-14599-00420080701,
Abstract = {Objectives: The two-fold purpose of this analysis is first to contrast the maternal risk factors and birth outcomes of American Indians (AIs) with other race/ethnic groups and to compare the maternal risk factors and birth outcomes of AIs by region to assess whether there are geographic variations in the adverse outcomes that might suggest intervention strategies. Study Design: This study used the National Center for Health Statistics live birth infant death cohort files from 1995-2001. Singleton live births to U.S. resident mothers were selected. The analyses were limited to non-Hispanic American Indians, including Aleuts and Eskimos (n = 239,494), Non-Hispanic White (n = 15,488,133), and Hstispanic births (n = 5,284,978). Results: This comparison of birth characteristics and outcomes by ethnic group revealed that AIs have more adverse maternal risk factors (e.g., unmarried and <18 years of age) than Whites and Hispanics. After adjustment for these factors, AIs have higher risks of low birth weight and preterm birth and elevated risks of postneonatal and infant mortality. Their cause-specific rates for perinatal, SIDS, injury and infection are also higher. The regional analysis indicated the South/Northeast have more low birth weight and preterm problems, but the Mid-West has the highest risks of infant mortality among LBW infants gestational age-specific mortality rates, and mortality from SIDS. Conclusions: These data show that AIs are not a homogenous group as evinced by distinct regional differences. SIDS is mainly a problem in the Mid-West, suggesting the involvement of environmental factors in that region. Further investigation is needed to examine the current AI perinatal health concerns. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Alexander, Greg R. and  Wingate, Martha S. and  Boulet, Sheree},
ISSN = {1092-7875, 1573-6628},
Journal = {Maternal and Child Health Journal},
Keywords = {pregnancy outcomes, American Indians, perinatal health concerns, maternal risk factors, birth outcomes, preterm birth, infant mortality, low birth weight, Birth Weight, Perinatal Period, Pregnancy, Racial and Ethnic Groups, Risk Factors, American Indians, Birth, Premature Birth},
Number = {Suppl1},
Pages = {S5 - S11},
Title = {Pregnancy outcomes of American Indians: Contrasts among regions and other ethnic groups.},
URL = {mslay@uab.edu},
Volume = {12},
Year = {2008},
}


@article{2014-06040-00120140201,
Abstract = {This article provides a brief preface to special issue on perceptual narrowing. This is a special issue on perceptual narrowing, the phenomenon that infants toward the end of the first year are worse than they were earlier during infancy in discriminating among items from non-native categories (e.g., other-race faces, animal faces, phonemic contrasts not used in the native language, foreign musical rhythms). Over the same time, infants maintain or improve their ability to discriminate items from native categories (e.g., own race upright human faces; phonemic contrasts used in the native language). Around 9-12 months, exposure to individual items from the unfamiliar category is sufficient to reverse the perceptual narrowing. Together, the articles featured in this special issue document the reality of the phenomenon of perceptual narrowing but make clear that the term does not describe a mechanism or the only developmental trajectory. (PsycINFO Database Record (c) 2014 APA, all rights reserved)},
