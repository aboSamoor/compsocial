ISSN = {0090-0036, 1541-0048},
Journal = {American Journal of Public Health},
Keywords = {racial disparities, ethnic disparities, children identification, autism spectrum disorders, Autism, Ethnic Identity, Pervasive Developmental Disorders, Racial and Ethnic Differences},
