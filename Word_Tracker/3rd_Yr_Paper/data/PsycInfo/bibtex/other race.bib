Year = {1973},
}


@article{1999-04000-00519990501,
Abstract = {There is substantial empirical evidence suggesting that criminal justice personnel differentially treat Black (or, more generally, persons of other races and members of visible outgroups) and White citizens. This paper presents a theoretical explanation of this phenomenon called the frustration and negative cue model of unfavorable police treatment of "other-race" persons. A key feature of this new perspective is a focus on the nonverbal aspects of the interaction process between the 2 parties, and a focus on the unconscious, automatic, albeit biased, cognitive processes accompanying the interaction. The model posits frustration to be a necessary (though not sufficient) first condition for the emergence of unfriendly police behavior. More specifically, the model suggests that White police officers experience their encounters with Black interaction-partners as an aversive and frustrating event. This frustration is hypothesized to be a direct consequence of "interpersonal distance-disturbances" accompanying cross-cultural interactions. The model is discussed in detail and supported with empirical evidence. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Winkel, Frans Willem},
ISSN = {0160-2527},
Journal = {International Journal of Law and Psychiatry},
Keywords = {frustration & negative cue model of unfavorable police treatment of other race persons, Models, Police Personnel, Race and Ethnic Discrimination, Minority Groups},
Number = {3-4},
Pages = {273 - 286},
Title = {A frustration/negative cue model of unfavorable police treatment of black citizens: Discrimination based on automatic and biased signal processing.},
Volume = {22},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1999-04000-005&site=ehost-live&scope=site},
Year = {1999},
}


@article{2013-23393-00420130701,
Abstract = {A church-based health promotion program of the United Methodist Church was implemented to address the holistic health of adults 50 years and older (n = 142). African American congregation members (n = 65) were paired with white congregation members (n = 77) for a total of 12 groups. Over a year, biracial groups participated in weekly two-hour meetings. A one-group pretest-posttest design was used to determine impacts on participants’ religiosity, spirituality, and social support; and if impacts varied by race. At follow-up, program components were assessed. Tangible social support overall improved, and participants experienced meaningful socialization, spiritual opportunities, and improved perceptions of other race groups. (PsycINFO Database Record (c) 2014 APA, all rights reserved). (journal abstract)},
