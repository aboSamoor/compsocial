Year = {2009},
}


@article{1998-02902-00319980601,
Abstract = {A unique race relations course taught at the University of North Florida during the 1970s provided life-changing experiences for Black and White students. Taught by a licensed clinical psychologist, course requirements included small group discussions of racial issues, individual interactions of students and the instructor, readings about racial issues, personal journals about associated experiences, a visit to a historically Black college, and a week-long live-in stay in the home of the other race. The interactive nature of the course compelled students to explore and define personal racial convictions in significant depth. In addition to helping class members learn about the other race, the experience facilitated personal development and self-discovery. In retrospective studies, conducted 20 yrs later, former students described the course as having life-long, life-changing impacts and recommended that similar courses be taught today. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
