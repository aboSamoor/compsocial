Number = {3},
Pages = {515 - 528},
Title = {Face versus non-face object perception and the 'other-race' effect: a spatio-temporal event-related potential study.},
URL = {roberto.caldara@pse.unige.ch, bernard.renault@chups.jussieu.fr},
