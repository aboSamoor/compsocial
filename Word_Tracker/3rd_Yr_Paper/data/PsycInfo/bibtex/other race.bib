Number = {1},
Pages = {119 - 122},
Title = {Physician overestimation of patient literacy: A potential source of health care disparities.},
URL = {pakelly@bcm.edu},
Volume = {66},
Year = {2007},
}


@article{2009-01143-01220090101,
Abstract = {According to the American Cancer Society, African American men have a higher incidence of and mortality rate from prostate cancer than any other race. Early detection through screening improves prostate cancer survival rates. Yet compared to that of Caucasians, the prostate cancer screening rate is low among African Americans. To address these disparities, the Florida Department of Health (FLDOH) held statewide prostate screenings in selected cities, targeting African American men. The half-day program, held on a Saturday, included (1) a panel discussion by a urologist, a faculty member from a local university who specializes in prostate research, and a prostate cancer survivor; (2) prostate- specific antigen screenings; and (3) the NEC, which is the focus of this GEM. Participants were 21 African American men ranging in age from 37 to 78, most of whom (71%) had professional occupations. A prostate screening program in a faith based setting offers good potential for providing dietary guidance for prostate health to African American men. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
