Number = {1},
Pages = {B1 - B9},
Title = {A holistic account of the own-race effect in face recognition: Evidence from a cross-cultural study.},
URL = {jtanaka@uvic.ca},
Volume = {93},
Year = {2004},
}


@article{1970-03560-00119691201,
Abstract = {Explored recognition for faces of persons of own and other race in 20 black and 20 white undergraduates at both a predominantly black and a predominantly white university. 10 stimulus photographs each of black and white males were selected from a pool of stimulus photographs for recognition. The d' measure defined by signal detection theory was used as an index of discriminability/acuity. White faces were found more discriminable than black faces, and Ss were found to habe higher acuity for faces of own race. Questionnaire data suggest differential experience with persons of other race only for black Ss at the white university, and fail to show a relation of reported experience to recognition acuity. The hypothesis of greater heterogeneity of white faces was not tested. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
