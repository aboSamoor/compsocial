Keywords = {skin color, face recognition, individual variations, test recognition task, Caucasian faces, Black faces, Face Perception, Facial Features, Skin (Anatomy), Classification (Cognitive Process), Memory, Race (Anthropological)},
Number = {1},
Pages = {145 - 148},
Title = {The role of skin colour in face recognition.},
URL = {yair1@post.tau.ac.il},
Volume = {38},
Year = {2009},
}


@article{1995-12707-00119941201,
Abstract = {Examined the effects of race, sex, health locus of control (HLC), and the interactions of these variables on cardiovascular reactivity in 109 Black and 105 White children (aged 10–18 yrs). Ss completed the Multidimensional HLC scales for children, and Ss' systolic and diastolic blood pressures were measured at rest and during a reactivity task. Black boys had significantly higher scores on the powerful others (external HLC) subscale than did White boys and girls. Black boys also showed greater systolic blood pressure reactivity when they were low vs high on internality when compared to the other race/sex groups. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
