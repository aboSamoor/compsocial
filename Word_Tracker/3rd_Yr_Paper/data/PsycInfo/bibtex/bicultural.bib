Journal = {Hispanic Journal of Behavioral Sciences},
Keywords = {academic achievement, acculturative stress, coping factors, Whites, Latinos, self-perception, ethnic differences, cross-ethnic research, bicultural model development, Academic Achievement, Coping Behavior, Cross Cultural Psychology, Self Perception, Stress, Acculturation, Models, Racial and Ethnic Differences, Social Support, Whites, Latinos/Latinas},
Number = {3},
