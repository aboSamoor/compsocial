@inbook{2008-00399-00820010101,
Abstract = {Writing this chapter has been one of the most difficult tasks that I have undertaken in my career. Except for short "biobibs," I have never written about myself for the purpose of publication and, for that matter, any purpose. While I'm not a shy person, I highly value my privacy, and so sharing my life experiences, and in particular those that relate to my family, is not something I easily do; furthermore, my cultural upbringing underscored the fact that one should not go around "bragging" about one's personal achievements. These reasons aside, I think that the major reason for my seeking to avoid writing this chapter was having to face the emotions, especially the negative ones, that are an inherent part of one's solitary journey through the labyrinth of life. Having undertaken the task to write this chapter I have opted to examine different pathways in my life's journey that, directly or indirectly, brought me to the place in which I presently find myself. I believe that the journey that I have undertaken has been extremely challenging in more ways than one. The paths that I have followed have been bilingual and bicultural, if not tricultural, in nature; they have richly encompassed racial, ethnic, and sexual orientation perspectives. It is my hope that as you read this chapter you will interpret my journey through life from the perspective of such challenges and experiences. As I begin writing, I am faced with the reality that it is too easy to ramble when one is reminiscing about, remembering, and/or reconstructing the past in relationship to one's self. Given the limitations of length, in this chapter, I selectively present memories and recollections of my life in chronological progression. The ones that I have chosen to present are those that I believe have had the most significant impact in getting me here to this place in life in which I presently find myself. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (create)},
Address = {Casas, J. Manuel, Counseling/Clinical/School Psychology Program, Graduate School of Education, University of California, Santa Barbara, CA, US, 93106},
Author = {Casas, J. Manuel and  Ponterotto, Joseph G. and  Casas, J. Manuel and  Suzuki, Lisa A. and  Alexander, Charlene M. and  Ponterotto, Joseph G. (Ed) and  Casas, J. Manuel (Ed) and  Suzuki, Lisa A. (Ed) and  Alexander, Charlene M. (Ed)},
Booktitle = {Handbook of multicultural counseling (2nd ed.).},
ISBN = {0-7619-1984-8, 978-0-7619-1984-1, 0-7619-1983-X, 978-0-7619-1983-4},
Pages = {78 - 95},
Publisher = {Sage Publications, Inc},
Title = {I didn't know where I was going but I got here anyway: My life's journey through the labyrinth of solitude.},
URL = {casas@education.ucsb.edu},
Year = {2001},
}


@inbook{2001-01080-00620010101,
Abstract = {Assuming that the therapist is actively engaged in an ongoing learning process about diverse cultures, this chapter focuses on specific steps he or she can take during an assessment of clients of minority cultures and groups. These steps involve the use of multiple sources of information, including information from health care providers and significant others, and direct and indirect observations. In addition, to facilitate an understanding of the client's personal history, the therapist needs to obtain information about relevant cultural histories and stay aware of changing conceptualizations of illness and health in different cultures. Toward this end, it can be helpful to calculate the eras during which a client experienced significant developmental phases and then think about the sociocultural influences during these time periods. Equally important is the use of a client's preferred language and a deliberate search for culturally related strength at the individual, interpersonal, and environmental levels. The case example of a 40-yr-old bicultural woman who came to counseling for help in coping with chronic pain is presented. (PsycINFO Database Record (c) 2015 APA, all rights reserved). (chapter)},
Author = {Hays, Pamela A.},
Booktitle = {Addressing cultural complexities in practice:  A framework for clinicians and counselors.},
ISBN = {1-55798-768-8},
Pages = {89 - 109},
Publisher = {American Psychological Association},
Title = {Sorting things out: Culturally responsive assessment.},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2001-01080-006&site=ehost-live&scope=site},
Year = {2001},
}


@article{2006-07643-00120060101,
Abstract = {I am honored to undertake the challenge of Editor for Art Therapy: Journal of the American Art Therapy Association as it begins its twenty-third year of publication. As Art Therapy faces its twenty-third year, we open a new chapter on the rich and varied history of art therapy ideas, personalities, dreams and struggles from which a profession emerged. We recall the visionaries who established the American Art Therapy Association, its scholarly journal, and research that created the beginnings of shared language, theories, and conceptions of practice. In this issue, readers will find references to the formative past still very much alive in our present: people like the late Joan Kellogg whose mandala studies inspired Phyllis Frame to apply its concepts to art therapy assessment of couples compatibility, commentary on the early history of the profession's formation, Chantel Lumpkin's contemplation of her path to professional identity as detoured by her culture, and Cliff Joseph who draws on his history as a minority art therapist in a politically charged historical time to reflect on the pressing need today for art therapists to become sensitive to the socio-political realities that breed the personal pathologies of their clients. Threaded through these contributions to the professional literature are questions relating to bicultural, assimilated or separated identities that will resonate with many art therapists who are questioning their own identification with a dynamically changing profession in these transitional times. Facing forward, Carol McNamee draws from the research of neuroscience to conceptualize art therapy as increasing bi-lateral brain functioning, bridging art therapy practice with language and concepts of science. Dan Horay introduces art therapists to new models of substance abuse treatment that break with the AA culture and its long-standing assumptions. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Kapitan, Lynn and  Kapitan, Lynn},
ISSN = {0742-1656},
Journal = {Art Therapy},
Keywords = {art therapy, scholarly journal, Art Therapy, Scientific Communication},
Number = {1},
Pages = {2 - 3},
Title = {Opening and Closing: Transitional Times in Art Therapy.},
Volume = {23},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2006-07643-001&site=ehost-live&scope=site},
Year = {2006},
}


@article{2000-12562-00320001001,
Abstract = {Few empirical studies exist that specifically assess the impact of family on the development of cultural identity (Reference Group Orientation = RGO) in individuals of mixed cultural heritage. Moreover, no studies exist involving individuals of dual-minority heritage and the rural Australian, especially the Indigenous experience. In this study, self-narrative (ethnographic interview) of Aborigines and Torres Strait Islanders with Chinese ancestry reveals the essential position of family in determining an individual's RGO outcome. Virtually all the participants acknowledge that the rich Indigenous cultural life-experience and upbringing provided by their families influenced their development of unquestioned, stable Indigenous RGOs. However, layered problem-free onto their core (Indigenous) RGOs are varying degrees of "other-culture" (here Chinese) identifications. The study argues that the differential impact of past and present government policies across the rural-urban divide has proved an obstacle to the formation of bicultural RGOs in mixed-heritage family members. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Ramsay, Guy M.},
ISSN = {1322-9400},
Journal = {Journal of Family Studies},
Keywords = {family, cultural identity & reference group orientation, Aborigines & Torres Strait Islanders with Chinese ancestry with rural vs urban upbringing, Ethnic Identity, Family, Indigenous Populations, Reference Groups, Self Concept, Chinese Cultural Groups, Culture (Anthropological), Rural Environments, Social Identity, Urban Environments},
