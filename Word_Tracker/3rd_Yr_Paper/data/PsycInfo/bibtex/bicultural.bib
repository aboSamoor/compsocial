Number = {2},
Pages = {69 - 86},
Title = {Who am I? The social construction of ethnic identity and self-perceptions in Latino preservice teachers.},
URL = {eclark@utsa.edu},
Volume = {33},
Year = {2001},
}


@article{2000-15918-00420000101,
Abstract = {Social workers, whether family practitioners or not, have historically begun their examination or analysis of what constitutes family with normative understandings. To more comprehensively intervene with families, social work practitioners need to begin with a broad understanding of family. Given US ethnic demographic shifts, and changes in economic trends, social workers will have to entertain a myriad of family form in their assessments, as they necessarily interact with a multiplicity of situations that require alternative models for understanding family. In this article, the author presents an historical overview of family research as organized by ethnicity. The author also presents an overview of theories on families that social workers may engage as they carry out their professional responsibilities. Implications for social work practice are discussed. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Méndez-Negrete, Josephine},
ISSN = {1044-3894},
Journal = {Families in Society},
Keywords = {ethnicity & theories on families & alternative models of family, implications for social work practice, Ethnic Identity, Family, Social Casework, Theories, Latinos/Latinas, Mexican Americans, Racial and Ethnic Groups},
Number = {1},
Pages = {42 - 48},
Title = {'Dime con quien andas': Notions of Chicano and Mexican American families.},
Volume = {81},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2000-15918-004&site=ehost-live&scope=site},
Year = {2000},
}


@article{1998-06215-00219980901,
Abstract = {Educational research relying on macrolevel variables shows a relationship between a group's economic status and its academic achievement. These findings are often used to provide a uniform, psychological description for a whole population, but they do not explain intragroup differences and the specific linkages between economic conditions and educational practices. The author reports on work conducted on poor Puerto Rican households in Philadelphia, showing the complex relationship between household members' survival strategies, residential mobility, home–school connections, and students' learning. The author also discussed how schooling and literacy practices are influenced by the structure and organization of the household unit and by the school context. The first part of the article provides the social context of the Puerto Rican community in Philadelphia. The 2nd section consists of 5 cased to illustrate how schooling and academic performance are influenced by the structure and organization of households and the school. Results suggest that household stability, which is influenced by economic stability, is the strongest factor affecting students' learning. Educational reform programs need to consider the economic conditions of these households even more than linguistic and cultural factors. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Tapia, Javier},
ISSN = {0161-7761, 1548-1492},
Journal = {Anthropology & Education Quarterly},
Keywords = {household stability & survival strategies & home–school connections, student learning & academic achievement, poor Puerto Rican families living in US, Academic Achievement, Family Structure, Poverty, School Learning, Latinos/Latinas, Immigration, Living Arrangements},
Number = {3},
Pages = {297 - 323},
Title = {The schooling of Puerto Ricans: Philadelphia's most impoverished community.},
