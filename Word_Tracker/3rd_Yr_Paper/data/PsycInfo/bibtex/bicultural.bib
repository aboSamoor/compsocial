@article{1980-06281-00119760201,
Abstract = {As an alternative to the traditional acculturistic model of education, a pluralistic model is presented designed to meet the needs of children from the low-income, urban Black ethclass. Compensatory education exemplifies the failure of the acculturistic model. Cultural deprivation continues to exist since it is not reasonable to believe the Black ethclass will become White and middle class because their teachers want or expect this to occur. Another problem is folklore about how Black children learn, e.g., (a) relationships between research methodology and outcomes of schooling, based on inappropriate research models; (b) speculation on relationships between teacher expectations and outcomes of schooling; (c) presumed relationships between learning and achievement motivation; and (d) preference of "radio-announcer" language (i.e., standard English) over Ebonics. G. West and J. White asked Black children and youth what kinds of experiences would enable effective coping. These themes emerged: an area of perceived mastery that would generate self-confidence; positive recognition of and reinforcement for perceived areas of mastery by peer group; and the opportunity to choose among alternatives for action in one's best interest. A set of moves for attaining these educational goals are recommended, including bicultural examination of interaction between Black children and schooling agents; more Black role models in education; data reflective of the Black frame of reference; evidence of the value Black children bring to schooling; and reorganization of delivery systems developing mastery, self-confidence, self-recognition, and belongingness. (10 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {White, Joseph L. and  Johnson, James A.},
ISSN = {0095-7984, 1552-4558},
Journal = {Journal of Black Psychology},
Keywords = {pluralistic vs acculturistic model of education, low income urban Black children, Blacks, Education, Lower Income Level, Urban Environments},
Number = {2},
Pages = {32 - 42},
Title = {Awareness, pride and identity: A positive educational strategy for Black youth.},
Volume = {2},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1980-06281-001&site=ehost-live&scope=site},
Year = {1976},
}


