URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2005-16266-005&site=ehost-live&scope=site},
Year = {2006},
}


@inbook{2005-14006-01420060101,
Abstract = {For many deaf children born to hearing parents, the best opportunity for learning a natural signed language will take place in their classroom, and the primary linguistic model will be their teacher. The goals of this chapter are to first highlight some of the social and linguistic practices that have been observed in deaf parent/deaf child and hearing parent/deaf child family contexts. Our review is influenced by a sociocultural theoretical framework in which engagement and guided participation support a child's development of language and identity. Second, we review what is known about classroom interactions in early childhood and elementary school contexts. Third, we discuss some practical considerations with respect to teachers emulating deaf caregiver--deaf child practices and consider some of the obstacles that may challenge this potential approach. Finally, taking together all of the research findings reviewed on caregiver--infant and teacher--student interaction, we end with a discussion in which we propose a new conceptualization of an educator of the deaf whose primary focus would be to guide and promote, through everyday practice, the bilingual and bicultural identity development of deaf children using communication strategies modeled upon effective family engagement. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
Address = {Singleton, Jenny L., Department of Educational Psychology, University of Illinois at Urbana-Champaign, 1310 South Sixth Street, 226 ED, Champaign, IL, US, 61820},
Author = {Singleton, Jenny L. and  Morgan, Dianne D. and  Schick, Brenda and  Marschark, Marc and  Spencer, Patricia Elizabeth and  Schick, Brenda (Ed) and  Marschark, Marc (Ed) and  Spencer, Patricia Elizabeth (Ed)},
