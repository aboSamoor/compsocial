Keywords = {Acculturation, American Indians, Home Environment, Hopelessness, Regional Differences, Rural Environments, Urban Environments},
Number = {1},
Pages = {68 - 76},
Title = {Patterns of hopelessness among American Indian adolescents: Relationships by levels of acculturation and residence.},
