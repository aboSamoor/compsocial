Number = {1},
Pages = {132 - 143},
Title = {Researching mental health in minority ethnic communities: Reflections on recruitment.},
URL = {jorun.rugkasa@psych.ox.ac.uk},
Volume = {21},
Year = {2011},
}


@inbook{2011-01469-01120110101,
Abstract = {Multicultural education is a curriculum reform movement expressive of the mainstream of the American sociopolitical tradition. As a reform movement multicultural education has attempted to redress issues of power relationships as expressed in the ideology of the long-established general education program inherent in most U.S. schools, but particularly as nested in the language arts and social studies curriculum. Given its narrative orientation, a paradigmatic definition of multicultural education is extremely difficult. A scholarly attempt at definition is as challenging as herding cats, because the movement is defined not in terms of a common agenda as much as it is defined in terms of opposition to the status quo. Fortunately, one of the more enduring categorizations of the multicultural tradition over the past two decades was created by Sleeter and Grant. Currently in its fifth edition, Making Choices for Multicultural Education draws on five approaches to multicultural education identified by Gibson: 1. education of the culturally different; 2. education about cultural differences; 3. education for cultural pluralism; 4. bicultural education; and 5. multicultural education. Sleeter and Grant, however, move beyond the approach of Gibson to create their own typology: 1. teaching the exceptional and culturally different; 2. human relations approach; 3. single-group studies approach; 4. multicultural education approach; and 5. multicultural social justice approach. The attraction of the Sleeter and Grant approach is that it attempts to capture the evolving nature of the multicultural education reform movement as it has refocused from issues of cultural exclusion to issues of power. This historical view of the multicultural education reform movement gives directionality to the drift of reform, and accordingly provides a vantage point from which to anticipate difficulties for the movement in the future. This chapter will use the Sleeter and Grant categorization as a template for surveying the movement, and as a vantage point for looking ahead. A second organizing principle for this chapter is the dimensional typology provided by James Banks. Banks identifies five dimensions of educational outcomes of particular relevance to multicultural education. These are: 1. content integration; 2. knowledge construction; 3. prejudice reduction; 4. equity pedagogy; and 5. empowering school culture. Drawing on these educational outcomes as they are expressed in each of the approaches of the Sleeter and Grant typology, processes of comparison across approaches will be facilitated. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
Author = {Brandhorst, Allan R. and  Totten, Samuel and  Pedersen, Jon E. and  Totten, Samuel (Ed) and  Pedersen, Jon E. (Ed)},
Booktitle = {Teaching and studying social issues: Major programs and approaches.},
ISBN = {978-1-61735-044-3, 978-1-61735-045-0, 978-1-61735-046-7},
Pages = {217 - 231},
Publisher = {IAP Information Age Publishing},
Title = {Multicultural education reform movement.},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2011-01469-011&site=ehost-live&scope=site},
Year = {2011},
}


@article{2005-00142-00720041201,
Abstract = {The present study tests and extends existing research about the effects of culture and self-construal on reports of accommodative dilemmas in dating relationships. Data were collected from members of three different regional cultures: U.S. mainland, Hawaii, and South Korea. Using a four-factor self-construal model, two hypotheses were posited: (i) members of collectivistic cultures tend to enact accommodation (loyalty, voice) more and nonaccommodation (neglect, exit) less than do their individualistic counterparts; and (ii) bicultural self-construal is more strongly associated consistently with accommodation than are the other three self-construal types. These hypotheses received support. Collectivism-individualism was related consistently with the type of accommodation, and the four self-construal types also displayed a reliable pattern of associations with accommodation and nonaccommodation. Post-hoc assessments revealed that the four-factor model predicted self-reported accommodation better than did the two-factor model used in previous research. Small effect sizes in the present study, however, suggest that other factors (e.g., industrialization) may have a considerable bearing on responses to accommodative dilemmas. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Yum, Young-ok},
ISSN = {0265-4075, 1460-3608},
Journal = {Journal of Social and Personal Relationships},
Keywords = {dating relationships, cultural differences, self-construal, accommodative dilemmas, collectivistic culture, individualistic culture, bicultural self-construal, Culture (Anthropological), Self Concept, Social Dating, Interpersonal Relationships, Acculturation, Cross Cultural Differences},
Number = {6},
Pages = {817 - 835},
Title = {Culture and self-construal as predictors of responses to accommodative dilemmas in dating relationships.},
URL = {youngok@ksu.edu},
Volume = {21},
Year = {2004},
}


@article{2011-24097-00120101201,
Abstract = {Bicultural and interreligious marriages always end up in divorce. Divorce rates are skyrocketing in Muslim communities. Muslims are immune from domestic violence because it is prohibited (or, alternatively, Muslims have high rates of domestic violence because it is sanctioned in the Qur'an). These are just some of the claims that are often circulated in Muslim communities, not only by community residents but even sometimes by imams and community leaders. Yet, what is the source of such claims? It certainly is not the scientific research literature. Unfortunately, mental health research has lagged behind the community’s need for valid and reliable answers to common questions and concerns. The theme of this issue of the Journal of Muslim Mental Health is marriage and family, illustrating how the burgeoning field of Muslim mental health is generating empirical research to address some of the community’s concerns and questions. Two of the articles examine controversial topics; namely, the Qur'anic verse 4:34, and current marriage and divorce trends in the United States. Two more articles focus on the virtually ignored topic of intercultural and interreligious marriages, introducing information on the motivations and experiences of North African Muslim–Italian Catholic marriages in Italy and Jewish-Arab marriages in Israel. The findings in all these articles pose significant challenges to the conventional "community wisdoms" listed at the beginning of this editorial and, as a whole, stimulate the scholarly discourse to search for answers at a more complex and meaningful level. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Amer, Mona M.},
ISSN = {1556-4908, 1556-5009},
