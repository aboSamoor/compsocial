Year = {2000},
}


@article{2000-00506-00320000801,
Abstract = {Examined the impact of a community-based intervention to prevent tobacco and alcohol use on parent–child communication among Hispanic migrant adolescents. 660 families participated in the randomized pre–post study, which also examined effects of adolescent age, gender, and acculturation, and household size. Treatment and control groups completed an 8-session culturally sensitive program presented by bilingual/bicultural college students. Parents attended 3 of the 8 sessions and helped their child complete homework. The intervention included: (1) information about tobacco and alcohol effects, (2) social skills training (i.e., refusal skills), and (3) the development of parent–child communication skills to support healthy youth decisions. Results showed significant intervention by household size interactions for parent and youth perceptions of communication, indicating that the treatment was effective in increasing communication in families with fewer children. Based on the effect size and the previously established relationship between communication and susceptibility to tobacco/alcohol use, it was determined that the intervention effect could mean a future 5–10% decrease in susceptibility for smaller families. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Litrownik, Alan J. and  Elder, John P. and  Campbell, Nadia R. and  Ayala, Guadalupe X. and  Slymen, Donald J. and  Parra-Medina, Deborah and  Zavala, Francisco B. and  Lovato, Chris Y.},
