Year = {1984},
}


@article{1983-33676-00119821201,
Abstract = {Discusses methods of assessing British bilingual children in order to ascertain (1) whether they have developed the interpersonal communications skills and the literacy-related skills necessary for their placement in an all English-speaking class, and (2) whether any failure to develop skills in English is due only to differences between the language spoken at home and that spoken in the classroom or to an actual learning disability. Hypothetical construct tests of general ability have been shown to be culturally biased, yet many educational psychologists still rely on these tests to determine the placement of bicultural children. Learning ability tests were developed to overcome this cultural bias, but this bias recurs in such factors as significance of the test to the child, motivation to complete the test, and the materials used. Further efforts to remove linguistic bias through the elimination of verbal interactions from such tests only succeed in making the tests less analogous to the classroom situation. Criterion-referenced tests have been developed to determine the rate of learning in the classroom. However, the success of such tests depends on the teacher knowing what the student has learned and what needs to be taught next. When the teacher or educational psychologist has sufficient background information on the bicultural child, assessment may be more accurate. (18 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Desforges, Martin},
Journal = {AEP (Association of Educational Psychologists) Journal},
Keywords = {assessment methods, bilingual children with suspected learning difficulties, Great Britain, Bilingualism, Educational Measurement, Learning Disabilities, Students},
Number = {10},
