@article{2010-21614-03320101101,
Abstract = {We collected similar data for another, ongoing study aimed at verifying the Unified Theory of Acceptance and Use of Technology (UTAUT), where the attitude towards computer use (ATCU) was additionally measured but not yet used for the model validation. The sample was larger, bicultural and the age of the participants varied in a wider range. The sample of this study consisted of n = 1891 participants, 61.1% female and 38.9% male, aged between 19 and 65. From these, 875 live in Germany and 1016 in Romania. All of them have gone through a university study at least equivalent to BA, either in a technical or in a non-technical profession. we found significant, strong and medium correlations of the ATCU with the performance expectancy (PE), the effort expectancy (EE) and the behavioral intention (BI). ATCU was found to be correlated with the other acceptance variables; however, ATCU does not seem to contribute to the overall variance of the dependent variables, neither plays a moderating role between the predictors and the BI. In conclusion, we support the assumption that the attitude concept is unnecessary for the acceptance models TAM and UTAUT. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Nistor, Nicolae and  Heymann, Jan Oliver},
ISSN = {0007-1013, 1467-8535},
Journal = {British Journal of Educational Technology},
Keywords = {computer usage, biculturalism, behavioral intention, Computers, Intention, Technology, Multiculturalism, Behavior},
Number = {6},
Pages = {E142 - E145},
Title = {Reconsidering the role of attitude in the TAM: An answer to Teo (2009a).},
URL = {nic.nistor@uni-muenchen.de},
Volume = {41},
Year = {2010},
}


@article{2010-23354-00820101001,
Abstract = {In this article the authors discuss first why it is crucial, from a clinical and public health perspective, to better understand the development as well as risk and protection processes for the mental health of immigrant children. The authors then shift focus to the main tenet of this article, namely, that specific aspects of the dual language development of immigrant children are highly relevant to their mental health and adaptation. This argument is illustrated with empirical studies on Latino immigrant children, as they represent the majority of immigrant children in America and as a way of exemplifying the risks and circumstances that are potentially shared by other immigrant groups. Finally, the authors conceptually differentiate dual language development and its mental health impact from the dual-culture (bicultural) development and circumstance of immigrant children and their mental health impact. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
