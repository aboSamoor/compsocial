Keywords = {acculturation, immigration status, depression prevalence, psychosocial factors, behavioral factors, medical history, Mexican Americans, older adults, Sacramento Area Latino Study on Aging, Acculturation, Epidemiology, Immigration, Major Depression, Mexican Americans, Health, Health Behavior, Human Sex Differences, Psychosocial Factors, Latinos/Latinas},
