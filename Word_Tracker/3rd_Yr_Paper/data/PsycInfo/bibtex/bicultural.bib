Volume = {60},
Year = {2013},
}


@article{2015-05624-00420150201,
Abstract = {The current article presents the stories of emerging feminist identities in context of multi-colored feminisms. Feminism for us has not become a separate lens through which we see the world; rather, it has become a tool we can use to blur those borders, facilitating an ease of integration, emphasizing our reality of intersectionality, without guilt or shame or overwhelming confusion. Acquisition of this new lens, this new layer through which we are now able to see the world, has resounded with us in a way that speaks truth, that shows truth. We are continuously on this path toward integrating our identities, crossing the borders without fear or destruction of what little certainty we have obtained as young, single, heterosexual, bicultural, bilingual, Catholic, Latina feminists. (PsycINFO Database Record (c) 2015 APA, all rights reserved)},
Author = {Martinez, Susana and  Mejia, Claudia},
