Keywords = {bilingual learning, mainstream classroom, curriculum, cognitive benefits, cultural benefits, English, mother tongue, Bilingual Education, Cognitive Development, Curriculum, Mainstreaming (Educational), Sociocultural Factors, Classrooms, English as Second Language, Native Language},
Number = {2},
