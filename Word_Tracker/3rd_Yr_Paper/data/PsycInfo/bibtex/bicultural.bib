Pages = {247 - 257},
Title = {Complexity of culture: The role of identity and context in bicultural individuals' body ideals.},
URL = {fionalee@umich.edu},
Volume = {18},
Year = {2012},
}


@article{2010-07945-00120100401,
Abstract = {Cross-cultural management research typically assumes that individuals have only one cultural profile. However, given the changing patterns in the world’s workforce it is increasingly possible that more employees and managers will be bicultural. This special issue responds to the need to further our understanding of this emerging demographic in organizations. In this introductory article, we provide a brief review of what we know about bicultural individuals, point out some implications of our current knowledge for organizations, identify opportunities for further exploration of these topics, and introduce the articles in the special issue. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
