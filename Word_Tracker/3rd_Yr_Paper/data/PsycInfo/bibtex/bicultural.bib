@article{2009-16971-00320091001,
Abstract = {The authors propose that culture affects people through their perceptions of what is consensually believed. Whereas past research has examined whether cultural differences in social judgment are mediated by differences in individuals’ personal values and beliefs, this article investigates whether they are mediated by differences in individuals’ perceptions of the views of people around them. The authors propose that individuals who perceive that traditional views are culturally consensual (e.g., Chinese participants who believe that most of their fellows hold collectivistic values) will themselves behave and think in culturally typical ways. Four studies of previously well-established cultural differences found that cultural differences were mediated by participants’ perceived consensus as much as by participants’ personal views. This held true for cultural differences in the bases of compliance (Study 1), attributional foci (Study 2), and counterfactual thinking styles (Study 3). To tease apart the effect of consensus perception from other possibly associated individual differences, in Study 4, the authors experimentally manipulated which of 2 cultures was salient to bicultural participants and found that judgments were guided by participants’ perception of the consensual view of the salient culture. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Zou, Xi and  Tam, Kim-Pong and  Morris, Michael W. and  Lee, Sau-lai and  Lau, Ivy Yee-Man and  Chiu, Chi-yue},
ISSN = {0022-3514, 1939-1315},
Journal = {Journal of Personality and Social Psychology},
Keywords = {culture, cross-country comparison, norms, priming, perceptions, consensual beliefs, personal values, Attitudes, Cross Cultural Differences, Personal Values, Social Norms, Judgment, Priming},
Number = {4},
Pages = {579 - 597},
Title = {Culture as common sense: Perceived consensus versus personal beliefs as mechanisms of cultural influence.},
URL = {xz2123@columbia.edu, mwm82@columbia.edu},
Volume = {97},
Year = {2009},
}


@article{2005-15406-01520051201,
Abstract = {Reviews Learning from My Mother's Voice: Family Legend and the Chinese American Experience by Jean Lau Chin (2005). The author provides a fascinating account of the immigration experience by integrating Chinese legend and personal experience. Weaving together themes of Chinese and Western mythology, bicultural identity, and common immigrant experiences of loss, guilt, and shame, Chin unveils the pain and transformation of immigration through her eyes and those of her mother, Fung Gor Lee. Overall, the author eloquently integrates personal experience and educational teachings into a powerful memoir that takes the reader into the household of Jean Lau Chin. Through her recounting of the struggle of intergenerational conflict and parent-child misunderstandings, shame is openly discussed in the context of immigration and ethnic identity transformation. The discussion of the progression from shame and misunderstanding of one's Chinese values, to respect and admiration, is particularly effective in demonstrating this process of transformation. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
