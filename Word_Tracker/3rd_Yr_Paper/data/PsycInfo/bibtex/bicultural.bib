URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1998-06215-002&site=ehost-live&scope=site},
Year = {1998},
}


@article{1995-70991-00119861101,
Abstract = {Provides information related to the social, linguistic, and psychological character of bilingual children; analyzes how bilingual programs are operated; and offers recommendations related to the education of bilingual students, particularly at early education levels. Detailed conclusions on the relationship between the bilingual character of the child and cognitive functioning remain tentative, since bilingual children have scored both higher and lower than monolingual children and since balanced bilingual children have outperformed both monolingual and nonbalanced bilingual children on specific cognitive tasks. The bilingual child is seen as possessing 2 language systems developed in a complex social milieu, coming to school at a time when cognitive and academic growth are most influenced by social milieu, a factor which may explain discrepancies in research outcomes. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
