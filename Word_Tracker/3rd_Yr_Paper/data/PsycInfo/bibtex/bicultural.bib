Pages = {199 - 213},
Title = {The family and cultural identity in aborigines and Torres Strait islanders of Chinese ancestry: A rural–urban divide.},
Volume = {6},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2000-12562-003&site=ehost-live&scope=site},
Year = {2000},
}


@article{2000-16041-00320001001,
Abstract = {Examined the acculturation levels of 99 Vietnamese college students (average age 21 yrs) living in or away from Vietnamese communities. The students, either born in Vietnam or in the US, had received precollege education and entered college in the US. In the study there were 2 instruments used: The Suinn-Lew Asian Self-Identity Acculturation Scale (SL-ASIA) and The Acculturation and Structural Assimilation Scale (ASAS). Results show that the community setting of residency at college was related to the Vietnamese college students' acculturation into US culture. The small size of the acculturation differences between the groups living in the 2 community settings was not surprising, because the respondents had completed most of their acculturation before college. Participants who lived in a Vietnamese community were more acculturated than those who lived in a non-Vietnamese community at college. The Vietnamese students living in a non-Vietnamese community may have more difficulty in being bicultural, which can be viewed as being different. Being different is often associated with being lonely or isolated and may reduce these students' freedom to acculturate. Authors also found that the longer the Vietnamese students had lived in the US, the more acculturated they became. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Duan, Changming and  Vu, Paul},
ISSN = {0883-8534, 2161-1912},
Journal = {Journal of Multicultural Counseling and Development},
