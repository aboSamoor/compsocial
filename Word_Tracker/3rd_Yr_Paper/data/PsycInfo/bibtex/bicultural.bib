}


@article{2011-21740-00520110901,
Abstract = {Reviews the book, Meaningful inconsistencies: Bicultural nationhood, the free market, and schooling in Aotearoa/New Zealand by N. M. Doerr (2009). The book is a clarification of the multiplicity of positions constructed by neoliberal reformists to sell education as a commodity to individuals and individual institutions in the worldwide market place. The author focuses on how the English language of the prevailing or mainstream New Zealand European/Pakeha culture and indigenous Maori language and culture are dealt with differently in secondary schooling. The book highly recommend to indigenous and minority group intellectuals, politicians, civil servants, other professionals, and spin doctors to improve their understanding of education as a commodity and to improve their understanding of biculturalism, citizenship, and nationhood in Aotearoa/New Zealand. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
