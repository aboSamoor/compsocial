Keywords = {dating, second generation Patels, bicultural identity, Indian Americans, parental attitudes, gender structures, sex role attitudes, human sex differences, young adults, Immigration, Social Dating, Social Identity, Multiculturalism, Generational Differences, Adult Attitudes, Adult Offspring, Ethnic Identity, Human Sex Differences, Parental Attitudes, Sex Role Attitudes, South Asian Cultural Groups},
Number = {4},
Pages = {571 - 588},
Title = {'Sshh...!! Don't tell my parents': Dating among second-generation Patels in Florida.},
