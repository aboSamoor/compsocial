@inbook{2001-01575-00220010101,
Abstract = {Four theoretical models that may explain the effects of acculturation and language use on delinquency were examined based on a sample of Chinese Canadian youths (aged 10–20 yrs old). They were social control, personal dysfunction, culture conflict, and bicultural models. The results revealed that acculturation increased delinquency involvement whereas bilingualism reduced it, particularly for foreign born respondents. These findings lent some support to the social control and bicultural models. However, the observed correlations between acculturation and social control factors were weak, and acculturation still showed an independent effect on delinquency after controlling for social control, thus suggesting that social control may not explain the effect of acculturation on delinquency. Put together, the results were most consistent with the bicultural model and pointed to the importance of balancing adherence to one's ethnic culture and acculturation to the host society. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
Author = {Wong, Siu Kwong and  Columbus, Frank and  Columbus, Frank (Ed)},
Booktitle = {Advances in psychology research, Vol. V.},
ISBN = {1-56072-953-8},
Pages = {55 - 70},
Publisher = {Nova Science Publishers},
Title = {Acculturation, language use, and Chinese delinquency: An examination of four theoretical models.},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2001-01575-002&site=ehost-live&scope=site},
