Title = {Reflections on identity.},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2010-13577-013&site=ehost-live&scope=site},
Year = {2010},
}


@inbook{2010-13577-00220100101,
Abstract = {The oral/manual "methods" controversy arose more than 200 years ago. Although many variations exist, there have been three basic approaches. An "oral" approach concentrates on the development of the spoken language of a community. What is now known as a bilingual-bicultural (Bi-Bi) approach emphasizes the development of the natural sign language of a community as the first language, then teaches the majority language through reading. A third approach supports the development of a sign system based on the syntax of a spoken language and modifications of a sign language in instruction. This system can be used either alone or in coordination with speech, known as simultaneous communication (Sim Com), or alone. A fourth approach, known as total communication (TC), encourages the use of all forms of communication, dependent on individual needs. These longstanding debates have not been resolved after two centuries, and represent different perceptions of deafness, the requirements for leading a full, rich life, and resultant educational and social goals. The oral method was dominant from 1880 to 1960. Since then, constant change has occurred, with sign languages and sign systems receiving significant support. Recent global developments in neonatal screening, early intervention programs, cochlear implants, and the growth of an "inclusion" model in education have major implications for instruction and for the development of language and communication skills in deaf individuals. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
Author = {Moores, Donald F. and  Marshark, Marc and  Spencer, Patricia Elizabeth and  Marshark, Marc (Ed) and  Spencer, Patricia Elizabeth (Ed)},
Booktitle = {The Oxford handbook of deaf studies, language, and education, Vol 2.},
ISBN = {978-0-19-539003-2},
Pages = {17 - 30},
Publisher = {Oxford University Press},
Series = {Oxford library of psychology},
Title = {The history of language and communication issues in deaf education.},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2010-13577-002&site=ehost-live&scope=site},
Year = {2010},
}


@inbook{2010-08787-01420100101,
Abstract = {Japan has long been a figure in the education drama spotlight. Educators from many other countries, particularly the United States have looked, often with awe and envy, at: what they thought was an "ideal" way to run an education system. However, there have been flaws in the research and it is obvious to anyone who spends a great deal of time studying Japanese education that while Japanese schools have great strengths, they also have glaring weaknesses. In my personal and professional life I have many non- Japanese friends and colleagues who are married and have children. When speaking with them, the conversation inevitably turns to education. "What are you going to do?" we ask, and the question elicits heavy sighs and headshaking. The answers are not easy. Most would like to send their child to an international school, from kindergarten through secondary school. Quite a number of these schools exist in major cities across the country. However, international schools are not cheap, and not always accessible, so in the end, many of my fellow expatriates send their kids to local elementary schools then leave the country before they would have to enter junior high. Although this is most certainly anecdotal, in my over two decades in Japan working with dozens of bicultural and transplanted people, I only know of two or three cases where expatriate parents willingly sent their offspring to local secondary schools or universities. It is those glaring weaknesses that cause my friends to return to their home countries and enroll their children there. The following chapter is a brief overview of the school system in Japan and an attempt to show recent transformations stemming from societal as well as administrational factors. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
Author = {Nishibata, Julia Christmas and  Sharpes, Donald K. and  Sharpes, Donald K. (Ed)},
Booktitle = {Handbook on international studies in education.},
ISBN = {978-1-60752-383-3, 978-1-60752-384-0, 978-1-60752-385-7},
Pages = {219 - 232},
Publisher = {IAP Information Age Publishing},
Title = {The current state of affairs in Japanese education: Schooling in flux.},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2010-08787-014&site=ehost-live&scope=site},
Year = {2010},
}


@article{2006-04144-01020060201,
Abstract = {Reviews the book, Rituals in Families & Family Therapy, Revised Edition by Evan Imber-Black, Janine Roberts, and Richard A. Whiting (2003). This is a new edition of the book originally published in 1988. The changes are modest, with many chapters virtually unchanged. Chapter 5 is new. It is a discussion of rituals for bicultural couples and families, by Lascelles W. Black. This will be useful for those working with families drawn from different backgrounds and cultures. But the impression that much of this book has not been as rigorously revised as it might have been. Nevertheless it remains a valuable part of the family therapy literature. This edition is a useful resource for family and other therapists. Those who own the first edition may not wish to purchase this new edition. Borrowing the book from a library to read the three new chapters may suffice. If you are not familiar with the first edition, this new edition may well be worth buying. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
