@article{2008-12272-00720080801,
Abstract = {This study examined the development of bicultural voice in Latina/o preservice teachers. Researchers used survey, interview, and observational data to probe students' knowledge, beliefs, and orientations related to teaching culturally and linguistically diverse students. The researchers found that the bilingual cohort courses afforded students with opportunities to juxtapose personal narratives with broader social contexts, thereby allowing students to examine and critique the ideology and curricula of schools. The authors assert that cultivating social justice orientations in bilingual-bicultural preservice teachers is crucial to the empowerment of bilingual-bicultural teachers and their students. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
