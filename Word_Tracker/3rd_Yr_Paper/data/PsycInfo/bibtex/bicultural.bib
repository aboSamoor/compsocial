Year = {2015},
}


@article{2013-26815-00120130101,
Abstract = {Reviews the book, A Borderlands View on Latinos, Latin Americans, and Decolonization: Rethinking Mental Health by Pilar Hernández-Wolfe (see record [rid]2013-07911-000[/rid]). The book is very loosely divided into six chapters that focus on defining borderlands, colonization and decolonization, new ways of thinking of the borderlands, trauma, healing, and a final catchall chapter. The book succeeds by bringing the reader’s attention to the vast multiplicity of Latin@ circumstances (Latin@ is a construction that resists the gendered Spanish endings yet avoids the complexity of Latino/a), especially when one considers the innumerable ways in which bicultural identity is lived by Latin@ people in the United States and around the world. The author calls this multifaceted bicultural existence living on the borderlands. Hernández-Wolfe provides interesting examples of lives that are lived across lines. Stories are connected to historical and social conditions. This book teaches about broad political and social circumstances that affect people (e.g., prejudice against homosexuality in the Latin@ community), but not much about how one can expect these issues to affect Latin@ people psychologically. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Bardi, C. Albert},
ISSN = {1554-0138},
Journal = {PsycCRITIQUES},
Keywords = {trauma, healing, bicultural identity, political circumstances, social circumstances, Latinos, Latin Americans, decolonization, mental health, Latin@, borderlands, Ethnic Identity, Mental Health, Multiculturalism, Latinos/Latinas, Politics, Sociocultural Factors, Colonialism},
