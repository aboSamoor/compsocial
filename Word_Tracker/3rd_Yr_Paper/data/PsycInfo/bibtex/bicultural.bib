Volume = {53},
Year = {2006},
}


@article{2005-16548-00720060101,
Abstract = {Counselors, academic advisors and educators working in educational institutions must address the increasingly diverse mental health and academic needs of culturally diverse groups. In Hawai'i, Native Hawaiians are a cultural minority. To alleviate the negative effects of current culturally inappropriate counseling and teaching methods, university personnel would benefit from intercultural training. The Intercultural Sensitizer (ICS) is a well-researched tool used in attribution training. The purpose of this study was to develop a Hawaiian ICS to train non-Hawaiian university personnel. The researcher gathered critical incidents from Hawaiian university students (N = 113); presented 20 edited, critical incidents to a bicultural Hawaiian panel (N = 6) to elicit culturally relevant attributions, and investigated the meanings Hawaiian students and non-Hawaiian students (N = 285) attributed to the critical incidents. Statistical tests indicated 3 of 20 critical incidents and 14 attributions were statistically significant, revealing finite, cultural differences between the Hawaiian and non-Hawaiian samples. Counseling and educational implications are suggested that should increase the chances of success among Hawaiian students enrolled in college. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Cruz, Kenoalani C. K. Dela and  Salzman, Michael B. and  Brislin, Richard and  Losch, Naomi},
ISSN = {0147-1767},
Journal = {International Journal of Intercultural Relations},
Keywords = {native Hawaiians, intercultural interactions, higher education, counselors, academic advisors, mental health, cultural minority, Counselors, Cross Cultural Differences, Hawaii Natives, Higher Education, Mental Health, Cross Cultural Communication, Minority Groups},
Number = {1},
Pages = {119 - 140},
Title = {Hawaiian attributional perspectives on intercultural interactions in higher education: Development of an intercultural sensitizer.},
URL = {noa@hawaii.edu},
Volume = {30},
Year = {2006},
}


@inbook{2006-09690-00520060101,
Abstract = {Trafimow, Triandis, and Goto (1991) introduced the idea that people can have more than one self-concept. They used a priming manipulation to increase the accessibility of either the private or collective self, and obtained effects on the retrieval of self-cognitions. Since that time, there have been numerous advances in our understanding of the accessibility of self-concepts. These include the invention of new priming methodologies, the application of these methodologies to a variety of dependent variables, the use of culture and language to prime different self-concepts, the priming of self-related mental processes as well as self-related mental content, and the proposal of additional self-concepts. In addition, theorizing about the self has been applied to bicultural individuals. The present chapter reviews the original literature that opened up this area for investigation, advances that have been made since that time, and possible future directions. Throughout, there will be a heavy emphasis on the interplay between cultures and self-concepts. (PsycINFO Database Record (c) 2015 APA, all rights reserved). (chapter)},
Author = {Trafimow, David and  Clayton, Krisstal D. and  Prescott, Anne P. and  Prescott, Anne P. (Ed)},
Booktitle = {The concept of self in psychology.},
ISBN = {1-59454-982-6},
Pages = {113 - 127},
Publisher = {Nova Science Publishers},
Title = {The self in its cultural context: Effects on content and processes.},
