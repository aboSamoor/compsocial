Publisher = {John Wiley & Sons Inc},
Title = {Becoming Bilingual, Biliterate, and Bicultural.},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2006-08778-003&site=ehost-live&scope=site},
Year = {2006},
}


@article{2000-12663-00620001201,
Abstract = {Explores the we-self and I-self relationships of the Asian and Western cultures, respectively, discussed in I. Matte-Blanco's theory of the mind (1988) from the perspective of 2 creation myths, asymmetrical and symmetrical modes of thought, and an example of bicultural therapy. Specifically, the author discusses the Hindu myth of creation and compares it to the Judeo/Christian myth. Interactions between creation myths, culture and Matte-Blanco's theory, are discussed, as well as cultural/mythological determinants of personality, cross cultural understandings of the mind, and the relationship between culture and the mind. Differences in asymmetrical and symmetrical modes of thought are illustrated in a case report describing a 33 yr old woman from India presenting with depression and the idea that she was being punished for something that her ancestors had done. It is concluded that the connection between archetypal creation myths and the ties between people in different cultures is clarified by Matte-Blanco's theory. All—creation myth, relationships, and Matte-Blanco theory—are vital to our understanding of the continuous process of self-creation. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
