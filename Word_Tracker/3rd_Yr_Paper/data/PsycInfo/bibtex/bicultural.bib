@article{1998-03270-00419980101,
Abstract = {Presents a literature survey on the identity of Asian young people in Britain. One of the main findings to emerge concerns the saliency of ethnicity. Ethnicity seems to be rooted in religion and language, which in many instances is only symbolic in nature, i.e., the young people are not always practicing their religion, nor are they learning to read and write their ancestral language. Ethnicity is instead largely attributed to 2 factors: in-group belongingness and the experience of racism. It emerges that ethnic identity develops as does the ego-identity, and that E. J. Marcia's model, as adapted by S. J. Phinney (1996), is conceptually sound and a pragmatic procedure to understand ethnicity. It appears that a majority of the young people are learning to be bicultural and bilingual (at a spoken level) and are developing 'hyphenated' identities. At the one end of the 'assimilation–separation' continuum, are the Hindu-origin young people, who are likely to seek assimilation to the British way of life, while at the other end are the Pakistanis and Bangladeshis who are prone to emphasize their distinctive Muslim identity and to seek merely a form of accommodation with their host culture. Young people of Indian background are pursuing a 'middle way' of integration between the 2 cultures. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Ghuman, Paul A. Singh},
ISSN = {0267-3843, 2164-4527},
Journal = {International Journal of Adolescence and Youth},
Keywords = {ethnic identity & acculturation to British culture, Asian adolescents, Acculturation, Asians, Ethnic Identity},
