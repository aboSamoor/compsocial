@inbook{1999-02535-00119990101,
Abstract = {Alita was a 27-yr-old Native American woman undergraduate student of mixed tribe ancestries who was self-referred to her university counseling center. For a portion of her life, Alita lived with Caucasian foster parents. She displayed identity confusion and a lack of self-confidence in relation to personal, bicultural, and academic issues. These guided the development and implementation of her treatment plan. After exploring Alita's belief and cultural traditions, it was determined that she felt comfortable with an integrated relational, cognitive-behavioral, psychoeducational, Native healing approach. The approach was free from an emphasis on diagnosis and pathology was emphasized. Alita was given information about depression, alcoholism, and meditation. She participated in cultural strengthening, which involved participation in the talking circle and sweats, cultural forums for attending to spiritual needs. Through these forums she found support for her spiritual needs as well as her Indian identity. Alita's fears about her drinking were addressed effectively with appropriate referrals to support groups in her community. The deferral of diagnosis for a while allowed other aspects of the intervention to take effect until Alita could make healthier choices about her drinking. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
Author = {Simms, Winona F. and  Jenkins, Yvonne M. and  Jenkins, Yvonne M. (Ed)},
Booktitle = {Diversity in college settings:  Directives for helping professionals.},
