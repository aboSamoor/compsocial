ISSN = {2329-0382, 2329-0390},
Journal = {Psychology of Sexual Orientation and Gender Diversity},
Keywords = {beauty standards, body satisfaction, gender identity, lesbian, sexual minority women, No terms assigned},
Title = {Exploring Biculturality and Beauty Standards Through Breast Discourse and Breasted Experience of Sexual Minority Women.},
