Keywords = {cuento modeling therapy vs traditional art/play therapy, trait anxiety, high risk Hispanic kindergartners through 3rd graders & their mothers, implications for development of culturally sensitive treatment methods, Anxiety, Ethnic Values, Imitation (Learning), Myths, Latinos/Latinas, At Risk Populations, Play Therapy, Psychotherapy},
Number = {5},
