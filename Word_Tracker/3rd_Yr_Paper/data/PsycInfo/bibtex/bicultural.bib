Title = {'Entre Dos Mundos' (Between Two Worlds): Bicultural Skills Training with Latino Immigrant Families.},
URL = {smokowsk@email.unc.edu},
Volume = {26},
Year = {2005},
}


@article{2008-04444-02020080501,
Abstract = {Studies of autobiographical memory have shown that the degree to which individuals focus on themselves vs. social relations in their memories varies markedly across cultures. Do the differences result from differing cultural self-views (i.e., an autonomous vs. a relational sense of self), as often suggested in the literature? Experimental evidence is required to answer this question. In the present study, Asian American participants (N = 118) were primed to focus on their American or Asian self prior to recalling important autobiographical events, and participants in a control group described things in nature prior to the memory recall. Those whose American self was activated recalled more self-focused and less socially oriented memories than those whose Asian self was made salient, with the control group falling in between. The findings shed light on the mechanism underlying cultural influences on autobiographical remembering. They further highlight the dynamic nature of the memory-self interplay in cultural contexts. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Wang, Qi},
ISSN = {0010-0277},
Journal = {Cognition},
Keywords = {bicultural self, autobiographical memory, Asian Americans, cultural contexts, social relations, Autobiographical Memory, Social Behavior, Sociocultural Factors, Asians},
Number = {2},
Pages = {743 - 751},
Title = {Being American, being Asian: The bicultural self and autobiographical memory in Asian Americans.},
URL = {qw23@cornell.edu},
Volume = {107},
Year = {2008},
}


@article{2008-00277-00120080101,
Abstract = {Reviews the book, Not grown up forever: A Chinese conception of adolescent development by Ching Man Lam (see record [rid]2007-13980-000[/rid]). In this book, the author reminds the reader that another way in which adolescent turbulence may be obviated is when parents keep a child "not grown up" indefinitely, encouraging dependence rather than the variety of autonomy that characterizes Western adolescence with its smorgasbord of identity choices. The insight the reviewer gained from reading Lam's book is that there is more than one way that cultural variations have obviated modal Western conceptions of adolescence. While much has been written about how cultures with defined puberty rites avoid the potential for adolescent turbulence by moving the child directly into adulthood, less has been said about cultures that strive to prolong dependency on parents. As such, this work describes an intriguing, apparently successful, alternative route for negotiating the path from childhood to adulthood, one that argues for cultural fidelity even for those adolescents who find themselves transplanted into a foreign cultural and psychological milieu. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Hatcher, Sherry Lynn},
ISSN = {1554-0138},
Journal = {PsycCRITIQUES},
Keywords = {bicultural adolescence, Chinese adolescents, adolescent development, cultural variations, Adolescent Development, Chinese Cultural Groups, Cross Cultural Differences, Sociocultural Factors},
Number = {17},
Title = {Negotiating a bicultural adolescence.},
Volume = {53},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2008-00277-001&site=ehost-live&scope=site},
Year = {2008},
}


@article{2008-00120-00220080101,
Abstract = {The authors examined the extent to which Asian American adolescents who were living in Hawaii adhered to Asian and European American cultural values in relation to mental health variables including collective self-esteem (membership, private, public, importance to identity), cognitive flexibility, general self-efficacy, and attitudes toward seeking professional psychological help. Results and implications for counselors are discussed (PsycINFO Database Record (c) 2013 APA, all rights reserved). (journal abstract)},
