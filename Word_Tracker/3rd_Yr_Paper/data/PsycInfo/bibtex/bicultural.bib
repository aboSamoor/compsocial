}


@article{2005-13932-01020050101,
Abstract = {Immigration from one country to another is a complex psychosocial process which often has a lasting impact on the individual's identity and interpersonal relationships. Migration of people from India to the USA, although traceable to the early twentieth century, began in the earnest only about four decades ago. Although different in certain emotional aspects, these two groups of immigrants from India shared a common fate. The poignant affects associated with immigration often mobilize creativity. The internal struggle was not limited to the immigrants themselves. Its repercussions could clearly be seen in their offspring. An annotated filmography of the cinema of Indian diaspora forms the centerpiece of this contribution. We have also included short, experimental films in an attempt to be comprehensive and representative of the entire effort in this realm. American Desi A romantic comedy about a college freshman discovering his Indian-American identity. The movies listed above contain the depiction of a vast range of affects related to immigration. These include the sweet wistfulness of nostalgia, the cutting pain of grief, the fumbling tenderness of acculturation, and the insistent quandary of bicultural identity. The importance of these movies, however, goes beyond their ascetic accomplishments. Our purpose in writing this paper has chiefly been psycho-education. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Tummala-Narra, Pratyusha and  Bewtra, Aruna and  Akhtar, Salman},
ISSN = {1742-3341, 1556-9187},
Journal = {International Journal of Applied Psychoanalytic Studies},
Keywords = {annotated filmography, psychosocial process, immigration, interpersonal relationships, migration, individual identity, Indian Diaspora, Films, Human Migration, Immigration, Interpersonal Interaction, Interpersonal Relationships, Psychosocial Factors, Self Concept},
Number = {3},
Pages = {297 - 310},
Title = {The Celluloid Ganges: An Annotated Filmography of the Indian Diaspora.},
URL = {utummala@umich.edu},
Volume = {2},
Year = {2005},
}


@book{2005-09514-00020050101,
Abstract = {Understanding Intercultural Communication offers a comprehensive look at foundational concepts of intercultural communication. Through its dynamic theme of flexible intercultural communication, authors Stella Ting-Toomey and Leeva Chung bridge the gap between intercultural communication theory and practice. This book is distinctive because of its balance between cross-national intercultural communication issues and U.S. domestic diversity issues. Written in a conversational style, the text presents up-to-date, identity-based frameworks and fresh approaches. By integrating current empirical research with lively intercultural examples, the authors ask thought-provoking questions throughout the text and pose intercultural ethical dilemmas for students to ponder. It offers strong coverage of topics such as the process of ethnic and cultural identity change, shock and intercultural adjustment, intercultural verbal communication styles, conflict facework interactions, romantic relationships and raising bicultural children, global identity challenges, decision-making choices in intercultural ethics. Highlights include: diversity Jeopardy boxes, Double-Take personal narratives and stories, Know Thyself mini-assessments, Quick Poll reaction polls, Picture This intercultural-interethnic poems, and Snapshots, illustrating cultural diversity and culture shock. In addition, dialogue scenes, global news clips, practical intercultural toolkits, and checkpoints reinforce student learning. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (cover)},
Author = {Ting-Toomey, Stella and  Chung, Leeva C.},
ISBN = {1-891487-73-6},
