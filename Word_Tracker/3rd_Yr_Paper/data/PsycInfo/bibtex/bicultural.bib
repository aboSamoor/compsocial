Volume = {35},
Year = {2007},
}


@article{2007-18800-00620071201,
Abstract = {Reply by the current author to the comments made by Genevieve Rail (see record [rid]2007-18800-005[/rid]) on the original article (see record [rid]2007-18815-001[/rid]). We thank Rail for her uneasy work in translating our abstract. During the composition of earlier drafts, we grew increasingly aware of the tenuous status of our Anglocentric history of Quebec, which Rail checks in part with delectable folk lyrics. Rail's amendment to our admittedly brief history of English and French Canada is both poignant and welcomed. Her response offers a politics of particularity that situates and contests the essentialist terms in which Canadian bicultural mythologies appear in our original essay. If we understand Rail's response correctly, our engagement of irony does not work because of an Anglocentric, and thus partial, historical account of Quebec. In her commentary, Rail undercuts the imagined incommensurability of French colonial binaries by introducing the concept of message, which is an insightful point for negotiating irony. To trouble our singular history of Quebec, Rail draws on the term as an expression of hybridity, a "braiding" between the already unstable positionings of colonizer and colonized that is often idealized as an interruption of European colonialist hierarchies. In the context of Rail's wider critique, irony seems to be an "improper starting point" not necessarily because of its harmful potential but rather because of our "flawed" historical introduction. While Rail suggests that Dunky is only ironic within a dominant English-Canadian discourse, we believe Dunky targets and interrupts this very discourse. The mascot was quite legible, offensive, and sometimes amusing for some French-language bloggers of our study. It was not always unintelligible or nonsensical. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Brayton, Sean and  Alexander, Ted},
ISSN = {0741-1235, 1543-2785},
Journal = {Sociology of Sport Journal},
Keywords = {politics, sports, power relations, irony, criticism, Criticism, Politics, Sports, Interpersonal Control},
Number = {4},
Pages = {486 - 491},
Title = {''I'm a frog, you're a frog, kiss me': A commentary on Brayton and Alexander (2007)': A response to Rail.},
Volume = {24},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2007-18800-006&site=ehost-live&scope=site},
Year = {2007},
}


@article{2007-18920-00320071101,
Abstract = {Purpose: This was a pilot study to assess the usability and impact of a feedback-based environmental tobacco smoke (ETS) reduction approach for Latino families. Design: A one-group, pre-post design was used. Methods: Information from hair samples from children analyzed for nicotine was used to create three feedback versions presented in a counterbalanced fashion. The intervention was conducted by a promotora in the participants' homes, with mailed materials and over the phone. Fifty Latino parent-child pairs were recruited. Parents were Spanish-speaking smoking adults (typically the mother) who reported exposing their children to ETS. Intervention efforts included home visits to provide nicotine feedback and counseling, two mailers presenting alternative versions of the feedback, and a supportive telephone call delivered over a 6-week period. Pre-post assessments conducted by a bicultural measurement technician measured parent reports of children's ETS exposure and children's hair nicotine. Parents' liking/usability of the three feedback formats was measured at the post-assessment. Paired t-tests assessed pre-post changes in exposure outcomes. Results: Parents' reports of exposure and children's hair nicotine levels showed statistically significant reductions. Parents liked all three feedback formats, particularly a comparative graphic format. Conclusions: Consistency of the positive findings suggests that this feedback approach to ETS reduction warrants further study with a more rigorous design. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Woodruff, Susan I. and  Conway, Terry L. and  Elder, John P. and  Hovell, Melbourne F.},
ISSN = {0890-1171},
Journal = {American Journal of Health Promotion},
Keywords = {hair nicotine feedback, reduce Latino children's environmental tobacco smoke, tobacco exposure, Feedback, Nicotine, Smoking Cessation, Tobacco Smoking, Latinos/Latinas},
Number = {2},
Pages = {93 - 97},
Title = {Pilot study using hair nicotine feedback to reduce Latino children's environmental tobacco smoke exposure.},
URL = {swoodruff@projects.sdsu.edu},
Volume = {22},
Year = {2007},
}


@article{2004-17488-01719970701,
Abstract = {Originally published in Contemporary Psychology: APA Review of Books, 1997, Vol 42(7), 602-603. Rising against the threat of losing autonomous identification, Culture and Difference: Critical Perspectives on the Bicultural Experience in the United States (1995) contests the "simple notions of individual psychological theories of identity, liberal paradigms of pluralism, and unproblematic notions of two distinct cultural world views interacting". It confronts the social and economic inequalities that many bicultural communities of color experience. According to her Introduction, editor Antonia Darder wishes to give voice to significant concerns and issues that have not been addressed in traditional academic literature. Consequently, this book covers numerous domains. This book would be good reading for researchers and anyone else interested in a political discourse on biculturalism. Not only will it raise our awareness of the inequalities that bicultural communities face, it will also challenge the insidiously slippery bias in our own research and clinical work. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
