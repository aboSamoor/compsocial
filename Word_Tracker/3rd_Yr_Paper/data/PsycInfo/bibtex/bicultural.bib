@article{2002-17436-01120020801,
Abstract = {Notes that the children of immigrants possess bicultural identities that reflect their ethnic heritage and their membership in the wider community. For most, strong identification with the dominant culture creates a desire for full inclusion within it. In the case of visible minorities, however, physical dissimilarity is at times experienced as an "ethnifying" obstacle to assimilation or integration. One response to this challenge is compensatory alignment with the majority group when physical appearance is made salient. To demonstrate this phenomenon, the authors asked 94 Chinese Canadian participants (aged 17-25 yrs) to rate their liking of a set of abstract paintings. The ratings were made in relation to various normative anchors and either in the presence or absence of a mirror. As predicted, only participants in the presence of the mirror showed heightened conformity to the perceived European Canadian (majority group) norm. This tendency, however, was not matched with greater distancing from the perceived Chinese Canadian (minority group) norm. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Tafarodi, Romin W. and  Kang, So-Jin and  Milne, Alan B.},
ISSN = {0146-1672, 1552-7433},
Journal = {Personality and Social Psychology Bulletin},
Keywords = {physical dissimilarity, compensatory alignment, conformity, bicultural visible minorities, bicultural identities, Chinese Canadians, Acculturation, Conformity (Personality), Ethnic Identity, Minority Groups, Physical Appearance},
Number = {8},
Pages = {1131 - 1142},
Title = {When different becomes similar: Compensatory conformity in bicultural visible minorities.},
URL = {tafarodi@psych.utoronto.ca},
Volume = {28},
Year = {2002},
}


@article{2002-17436-00320020801,
Abstract = {In a study of bicultural individuals' self-perceptions, Chinese-born students were randomly assigned to participate in either Chinese or English. Serving as controls, Canadian-born Ss of either European or Chinese descent participated in English. A total of 111 Ss (aged 18-40 yrs) participated. Ss completed questionnaires. The effects of the language manipulation paralleled findings in previous studies comparing East Asians to North Americans. Ss responding in Chinese reported more collective self-statements in open-ended self-descriptions, lower self-esteem on the Rosenberg scale, and more agreement with Chinese cultural views than did the remaining groups. In their self-descriptions, Ss writing in Chinese provided similar numbers of favorable and unfavorable self-statements. The other groups reported more favorable self-statements. Ss reporting in Chinese indicated similar levels of positive and negative mood. The remaining groups reported more positive mood. The results suggest that East-Asian and Western identities may be stored in separate knowledge structures in bicultural individuals, with each structure activated by its associated language. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Ross, Michael and  Xun, W. Q. Elaine and  Wilson, Anne E.},
ISSN = {0146-1672, 1552-7433},
Journal = {Personality and Social Psychology Bulletin},
Keywords = {self-perception, bicultural individuals, Chinese-born students, language, Cross Cultural Differences, Culture (Anthropological), Language, Self Perception},
Number = {8},
Pages = {1040 - 1050},
Title = {Language and the bicultural self.},
URL = {mross@uwaterloo.ca},
Volume = {28},
Year = {2002},
}


@article{2001-09728-00220011001,
Abstract = {Explored how communication practices at Chinese language schools affected bicultural identity development for new Chinese immigrants and their children (aged 4-13 yrs). Ethnographic research at a Chinese school in Chicago involved participation, observation, and interviews. 35 parents were interviewed regarding: (1) reasons they send their children to the school, (2) communicative activities in which they participate, (3) impact of school activities on children's bicultural identity development and their own psychological well-being, (4) perceptions of Chinatown, and (5) suggestions for future development of the school. The results indicate a desire for cultural preservation as well as motivation for bicultural identity development and awareness of the benefits of biculturalism. Parents expressed a willingness to engage the larger society, but a rejection of an identity associated with Chinatown. This study is seen to challenge the conventional notion that middle-class Asian Americans are well assimilated into American society or that they are identified with their ethnic enclaves only. The author concludes that communication can transcend cultural identity, and suggests that biculturalism, as opposed to Americanization, is related to positive adjustment for immigrants and their children. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
