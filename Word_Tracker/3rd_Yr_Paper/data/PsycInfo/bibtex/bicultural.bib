@article{2000-07819-00120000701,
Abstract = {Examined how Korean-American acculturation is related to the 5 health dimensions of smoking, physical activity, fat intake, body weight, and self-reported health. 356 Ss (aged 17–90 yrs) completed a mailed questionnaire; acculturation was measured using the 2-culture matrix model (J. W. Berry;1980, 1992) based on theoretical work of M. M. Gordon (1964, 1978). Ss comprised distinct acculturated, bicultural and traditional groups, based on the 4 acculturation components of American structural, American cultural, Korean structural and Korean cultural. Results show that bicultural men were least likely to smoke, while acculturated and bicultural women were more likely to smoke than traditional women. Higher acculturation was related to light physical activity, but not to vigorous physical activity. Fat intake did not differ according to acculturation status. Higher acculturation was associated with higher body mass index and better self-reported health in men. Findings suggest that among Korean Americans, acculturation components vary in their relationships with health, beneficial health effects of being bicultural are found only for smoking status of men, and acculturation–health relationships are multidimensional. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Lee, Soo-Kyung and  Sobal, Jeffery and  Frongillo, Edward A. Jr.},
ISSN = {0277-9536, 1873-5347},
