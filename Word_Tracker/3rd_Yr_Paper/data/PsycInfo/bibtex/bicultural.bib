@inbook{2005-03477-01220050101,
Abstract = {Social work has had a long tradition of group work practice with diverse immigrant communities, starting before Jane Addams s Hull House in Chicago during the 1890s and continuing up until the present day. While this chapter will focus on specific issues of cross-cultural group work with Asian Americans from East Asia--primarily China, Korea, and Japan--the chapter will begin by briefly addressing the universal themes that all immigrants experience in order to maximize its relevance for practitioners. An important aspect of the immigrant experience is the development of a bicultural identity, or "the ability to move freely between one's ethnic community and the larger culture" (Chau, 1990, p. 2), which helps maintain the immigrants' self esteem and facilitates their integration into the dominant culture. An important difference between Asian and Western cultures is the Asian emphasis on nonverbal communication systems, unlike the Western culture, which is highly verbal (Roland, 1991; Sue & Sue, 1990). Consequently, silence, eye contact, physical distance, and other nonverbal signals constitute a subtle and highly developed form of communication in Asian cultures, and have to be carefully attended to and understood by the group leader. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
Author = {Ringel, Shoshanna and  Greif, Geoffrey L. and  Ephross, Paul H. and  Greif, Geoffrey L. (Ed) and  Ephross, Paul H. (Ed)},
Booktitle = {Group work with populations at risk (2nd ed.).},
ISBN = {0-19-515667-6},
Pages = {181 - 194},
Publisher = {Oxford University Press},
Title = {Group Work with Asian-American Immigrants: A Cross-Cultural Perspective.},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2005-03477-012&site=ehost-live&scope=site},
Year = {2005},
}


@article{2005-03360-00220050101,
Abstract = {This study examines Latino/a adolescents' ethnic identities and academic achievement. In open-ended interviews, the high school aged youth employed cultural and nationality explanations for their ethnic label choices. In many cases, they did not commit to a specific label, employing instead language that indexed their fluid, border identities. This study also made use of various academic performance measurements to quantitatively explore relationships between ethnic identity and academic achievement. A cultural dimension to ethnic identity and a bicultural identity emerged as significant predictors of academic performance. However, no single ethnic label had a definitive relationship with academic performance. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
