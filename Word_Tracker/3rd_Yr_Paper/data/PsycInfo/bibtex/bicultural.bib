Volume = {49},
Year = {2012},
}


@inbook{2011-26248-01520120101,
Abstract = {This chapter presents three conditions that threaten successful adaptation for many Latino youth: poverty and high-risk neighborhood environments, challenges associated with immigration and acculturation, and ethnic discrimination and stereotyping. Following our discussion of these risks and evidence for the role in the psychological adaptation of Latino youth, we discuss how three culturally linked protective resources—traditional familism values, ethnic identity, and bicultural competence—can potentially facilitate positive adaptation and resilient outcomes for Latino youth. Finally, we discuss how our current understanding of these risk and protective processes can be used to inform services, programs, and policies for this population. (PsycINFO Database Record (c) 2013 APA, all rights reserved). (chapter)},
