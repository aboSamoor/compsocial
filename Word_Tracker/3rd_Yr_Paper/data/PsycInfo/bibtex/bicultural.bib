}


@article{2001-14234-00220010201,
Abstract = {Examined the professional development history of 5 tenured African American women at a predominantly White university, to explore within a bicultural framework how minority professionals develop competencies to meet career expectations in White organizational cultures. Themes that emerged from interviews conducted with 5 45–51 yr old faculty members at a major research university in the Southwest included: the power of self-deception, finding a safe space, the power of knowledge and visibility, and life structure and bicultural competence. The findings indicate that race, culture, and identity play a vital role in the career development of minority professionals in majority organizations. An important finding is seen as the way the women accessed the power of their bicultural life structure to develop strategies for maintaining successful careers in White organizational cultures. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
