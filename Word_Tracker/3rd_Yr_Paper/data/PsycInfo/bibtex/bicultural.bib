ISSN = {1075-2730},
Journal = {Psychiatric Services},
Keywords = {ethnic matching, mental health services, ethnic minority clients, bilingual bicultural case manager, community care teams, psychiatric crisis teams, psychiatric inpatient services, Australia, Bilingualism, Case Management, Cultural Sensitivity, Mental Health Services, Minority Groups, Crisis Intervention Services, Mental Health, Racial and Ethnic Differences, Teams},
