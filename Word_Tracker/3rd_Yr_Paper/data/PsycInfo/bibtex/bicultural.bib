Number = {3},
Pages = {234 - 245},
Title = {Ethnic/racial diversity and posttraumatic distress in the acute care medical setting.},
URL = {dzatzick@u.washington.edu},
Volume = {71},
Year = {2008},
}


@article{2008-14247-00620080901,
Abstract = {In an effort to investigate the students' experience of African American Student Network (AFAM), we conducted a pilot study with 5 AFAM students. The pilot study focused on understanding AFAM through the eyes of the students. In-depth semi-structured interviews and an adaptation of interpretive phenomenological analysis were included to capture to students' experiences. The pilot study included two males and three females from 19-23 years old. Participation in AFAM ranged from 1 to 4 semesters. Year in school ranged from sophomore to senior. Three participants identified as African American, one identified as biracial, and one identified as bicultural (being a more recent African immigrant but also feeling connected to African American culture). The pilot study was based in qualitative research rooted in phenomenology—the study of the "lived" experience of a phenomenon— which emphasizes the in-depth exploration of the experiences of a small number of research participants. Seven themes were identified in students' experiences of AFAM: (a) a safe space, (b) connectedness, (c) validation, (d) resilience, (e) intellectual stimulation, (f) empowerment, and (g) a home base. Quotes exemplify the richness of each theme. The results of the pilot study provide some empirical evidence for how AFAM is meeting the needs of African American students on our campus. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
