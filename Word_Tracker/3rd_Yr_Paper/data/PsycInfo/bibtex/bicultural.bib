Year = {2014},
}


@article{2015-23221-01020140901,
Abstract = {Despite the increasing presence of Black immigrant students on college campuses, these students, and their educational experiences, are often overlooked because they are lumped in with Black/African American students. In response to such oversights, this qualitative study uses a dual socialization framework to explore how Black immigrant students negotiate the challenge of adjusting to a predominantly White institution (PWI) and identifies sources of support that may help these students make academic and social connections to the institution. Four themes are presented, highlighting the challenges and factors associated with bicultural experiences among Black immigrant college students. Implications for practice and future research are also discussed. (PsycINFO Database Record (c) 2015 APA, all rights reserved). (journal abstract)},
