Journal = {Journal of Cultural and Evolutionary Psychology},
Keywords = {multicultural literature, bicultural identity, expressive styles, Chinese Canadians, The Jade Peony, acculturation, Chinese Cultural Groups, Culture (Anthropological), Multicultural Education, Writing Skills, Acculturation, Cross Cultural Differences, Narratives, Multiculturalism},
Number = {1},
Pages = {51 - 76},
Title = {Cultural fusion, conflict, and preservation: Expressive styles among Chinese Canadians.},
URL = {dkuiken@ualberta.ca},
Volume = {4},
Year = {2006},
}


@inbook{2006-03629-01620060101,
Abstract = {The multicultural movement accentuates the ability of counselors from one culture being able to efficiently work with clients of other cultures. A strong emphasis has been on counselors of the majority culture and their proficiency to cross cultural boundaries to facilitate productive sessions for ethnic minority clients. While it has been a much needed emphasis due to the majority of counselors being Caucasian, it is time to extend a shift of paradigm in expectations to ethnic minority counselors, especially those who are bicultural. Bicultural counselors possess a unique advantage when working with matching ethnic clientele: they can advocate for clients and families of their own cultural group in ways that others cannot. There exists a great responsibility which must be shouldered by ethnic minority counselors to act as cultural brokers between the majority culture and their own minority culture. However, there is no emphasis on this responsibility nor a systematic pedagogy that addresses teaching ethnic minority counselors ways in which they can use their unique position to promote mental health among people of their same culture. Consequently, counselors from ethnic backgrounds who are bicultural are not fully utilizing the resources available to them to foster mental health among their own people. As a cultural broker, the ethnic minority counselor can use the self as an instrument by (1) endorsing the utilization of counseling services, (2) modeling health, and (3) developing other bicultural counselors. These methods are ways in which the field can move beyond the textbook knowledge of multiculturalism while implementing practical measures to reach often unreachable populations. (PsycINFO Database Record (c) 2013 APA, all rights reserved). (create)},
Author = {Kim, Josephine M. and  Walz, Garry R. and  Bleuer, Jeanne C. and  Yep, Richard K. and  Walz, Garry R. (Ed) and  Bleuer, Jeanne C. (Ed) and  Yep, Richard K. (Ed)},
Booktitle = {Vistas: Compelling perspectives on counseling 2006.},
ISBN = {1-55620-266-0},
Pages = {77 - 79},
Publisher = {American Counseling Association},
Title = {Ethnic Minority Counselors as Cultural Brokers: Using the Self as an Instrument to Bridge the Gap.},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2006-03629-016&site=ehost-live&scope=site},
Year = {2006},
}


