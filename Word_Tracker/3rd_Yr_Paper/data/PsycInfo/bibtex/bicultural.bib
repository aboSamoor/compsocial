Journal = {Journal of Transcultural Nursing},
Keywords = {Hispanics women's experiences, substance abuse, intimate partner violence, HIV, risk factors, Drug Abuse, HIV, Human Females, Intimate Partner Violence, Latinos/Latinas, Female Attitudes, Risk Factors},
Number = {1},
Pages = {46 - 54},
Title = {Hispanic women’s experiences with substance abuse, intimate partner violence, and risk for HIV.},
URL = {rosagonzalez@miami.edu},
Volume = {22},
Year = {2011},
}


@inbook{2011-23063-00620110101,
Abstract = {It is widely assumed that a strong sense of ethnic identity is associated with greater adjustment and well-being of American Indian/Alaska Native adolescents. Available evidence supports this proposition, but there are complexities related to ethnic identity development that support a call for further research in this area. This chapter addresses those complexities and provides a review of the extant literature within this context. We begin by delineating the scope of this chapter, identifying limitations and issues related to the study of development in American Indian/Alaska Native youth. Conceptual models of ethnic identity are then reviewed, followed by an articulation of unique dimensions of American Indian/Alaska Native identity and consideration of historical impacts on ethnic identity formation. The limited research on adolescent American Indian/Alaska Native ethnic identity is then reviewed, followed by discussion of several significant questions for future research. Given this critical review of theory and research, implications for applying this knowledge toward enhancing ethnic identity among American Indian/Alaska Native adolescents are suggested. This chapter provides a review of extant theory and research on ethnic identity among American Indian/Alaska Native adolescents. Before we begin that review, however, there are two relevant points of clarification that inform the scope of this chapter. First, confusion among different areas and overlap between related constructs make it particularly difficult to integrate the literature on American Indian/Alaska Native ethnic identity development. Terms such as ethnic identity, cultural identity, bicultural identity, acculturation, and enculturation are often used without clear specification of exactly what these constructs mean or how they are related to one another. Each of these constructs has at times included components of identification with American Indian/Alaska Native culture, sense of belonging to the cultural group, ethnic pride, participation in traditional activities, and endorsement of cultural beliefs and worldviews. We explicitly omitted racial identity in this chapter and will not stray into the extensive debate regarding its distinction from ethnic identity; the bulk of the racial identity research deals with African American populations and their unique experiences as a minority group in the United States, and is not directly relevant to a discussion of American Indian/Alaska Native ethnic identity. However, there are lessons to be learned from the racial identity literature that can inform our attempts to understand ethnic identity among American Indian/Alaska Native youth. Similarly, there exists an extensive literature on acculturation among immigrant groups (primarily various Latino and Asian groups), but these are also outside the scope of this chapter. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
Address = {Markstrom, Carol A., West Virginia University, PO. Box 6122, Morgantown, WV, US, 26506},
Author = {Markstrom, Carol A. and  Whitesell, Nancy and  Galliher, Renee V. and  Sarche, Michelle C. and  Spicer, Paul and  Farrell, Patricia and  Fitzgerald, Hiram E. and  Sarche, Michelle C. (Ed) and  Spicer, Paul (Ed) and  Farrell, Patricia (Ed) and  Fitzgerald, Hiram E. (Ed)},
Booktitle = {American Indian and Alaska Native children and mental health: Development, context, prevention, and treatment.},
ISBN = {978-0-313-38304-5, 978-0-313-38305-2},
Pages = {101 - 131},
Publisher = {Praeger/ABC-CLIO},
Series = {Child psychology and mental health},
Title = {Ethnic identity and mental health among American Indian and Alaska Native adolescents.},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2011-23063-006&site=ehost-live&scope=site},
Year = {2011},
}


@article{2010-26512-01120110101,
Abstract = {In this article we reflect on the recruitment of research participants to two related studies of experiences of mental health problems in Black and minority ethnic communities in the United Kingdom. A total of 65 people were recruited via three main strategies: the employment of bicultural recruiters, intensive information sharing about the studies, and work through local community groups. Three main issues seemed to affect recruitment: gatekeepers' attitudes, the (non)payment of participants, and reciprocal arrangements with local community groups. The type of strategy employed resulted in recruits with differing characteristics (although our sample was too small to draw generalizable conclusions). We conclude that to ensure that research participation is accessible to all, researchers must employ flexible recruitment methods that permit adaptation to specific needs arising out of health status, level of involvement with services, culture, and socioeconomic status. Systematic research into this part of the research process is needed. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
