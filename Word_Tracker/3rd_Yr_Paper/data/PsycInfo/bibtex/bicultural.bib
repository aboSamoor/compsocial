Title = {Inclusion, itinerant teachers, and the pull-out model.},
URL = {donald.moores@unf.edu},
Volume = {153},
Year = {2008},
}


@article{1998-02119-00919980501,
Abstract = {Reports an error in the original article by I. Cuellar et al ( Journal of Community Psychology, 1997 [November], 25 [6], 535–549). The order of authorship for this article was incorrect. The order should have been I. Cuellar, R. E. Roberts, B. Nyberg, and R. E. Moldonado. (The following abstract of this article originally appeared in record [rid]1997-43369-004[/rid].) The relations among ethnic identity, as measured by the Multigroup Ethnic Identity Measure, and behavioral aspects of acculturation, as measured by the Acculturation Rating Scale—II, were studied in 1,367 college freshmen (aged 17–60 yrs), 87% of whom were of Mexican origin. The results strongly support the concept that ethnic identity and acculturation are related but separate processes. Ethnic identity scores were found to be highest in 1st generation, less acculturated Ss, and traditional acculturative types. Higher levels of acculturation were associated with less feelings of affirmation and belonging, and less feelings of ethnic identity achievement. High bicultural Ss were found to obtain higher scores in ethnic identity than low bicultural Ss, and high bicultural Ss were found to be oriented more toward others than those Ss who were classified as traditional or assimilated… (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
