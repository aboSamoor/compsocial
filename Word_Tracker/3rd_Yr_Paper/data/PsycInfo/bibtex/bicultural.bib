Number = {3},
Pages = {464 - 475},
Title = {Religious practices in cross-cultural contexts: Indonesian male science students’ adjustment in Taiwan.},
URL = {yunglung@mx.nthu.edu.tw},
Volume = {62},
Year = {2015},
}


@article{2012-19737-00820120101,
Abstract = {Terror management theory posits that cultural worldviews provide protection against death-related anxiety. To the extent that worldviews often encompass competing beliefs, the present research investigated the effect of the salience of an incompatibility between worldview elements. French nationals of second- or third-immigrant generation (N = 193) were exposed to compatible or incompatible aspects of their cultural identities and then induced to contemplate their own death or a neutral topic. Participants reminded of their mortality renounced their ethnic identity more in the cultural incompatibility condition, and this effect was confined to those who initially presented an integrated (or bicultural) identity. Mortality salience led to monoculturalism striving when bicultural participants considered incompatible aspects of both their cultures, hence verifying the importance of upholding a strong and unwavering cultural worldview to cope with death awareness. The role of death-thoughts accessibility and religious attitudes following participants’ efforts to shore up their cultural worldview is discussed. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Goncalves Portelinha, Isabelle and  Verlhiac, Jean-François and  Meyer, Thierry and  Hutchison, Paul},
ISSN = {1016-9040, 1878-531X},
Journal = {European Psychologist},
Keywords = {bicultural identity, cultural incompatibility, multiculturalism, religiosity, terror management, Cross Cultural Differences, Death and Dying, Religiosity, Terror Management Theory, Multiculturalism, Ethnic Identity, Sociocultural Factors},
Number = {3},
Pages = {237 - 245},
Title = {Terror management and biculturalism: When the salience of cultural duality affects worldview defense in the face of death.},
URL = {isabellegoncalves@gmail.com},
Volume = {17},
Year = {2012},
}


@inbook{2011-30521-00320120101,
Abstract = {This chapter focuses on the intersection of environmental displacement and language education. The devastating earthquake in Haiti brought this intersection into sharp focus as large numbers of Haitians have been forced to migrate internally (causing new problems of access to potable water, sustainable food sources and safe habitats, which portend repeated environmental displacement) and internationally, particularly to the U.S. As the former often leads to the latter, this chapter examines the growing and potential presence of environmentally displaced people—also called "climate refugees," "ecological refugees," "environmental diaspora," "environmental migrants," and "environmental refugees"—among the English language learner population and, thereby, the impact of climate change, biospheric degradation, environmental dislocation and sustainability in bilingual-bicultural education curriculum theorizing. Specifically, I examine the issue of environmental dislocation as identity in bilingual-bicultural education and introduce Daisaku Ikeda's (1928-present) educational philosophy as an Eastern, transformative eco-educational approach to this complex and developing issue. As I have elsewhere both reviewed extant literature on environmental displacement and examined Ikeda's United Nations Decade proposal at length, those discussions will not be reiterated here (see Goulah, 2010a, 2010b). Instead, I focus on three of Ikeda's views as Eastern approaches required at this time of environmental and linguistic dislocation: cosmological identity as ecological selfhood, abstraction at the root of environmental and linguistic displacement, and value-creating pedagogy as a new current in bilingual-bicultural education. First, however, I examine the state of environmental displacement and its interrelation with English language learners in the United States. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
Author = {Goulah, Jason and  Oxford, Rebecca L. and  Lin, Jing and  Oxford, Rebecca L. (Ed) and  Lin, Jing (Ed)},
Booktitle = {Transformative eco-education for human and planetary survival.},
ISBN = {978-1-61735-502-8, 978-1-61735-503-5, 978-1-61735-504-2},
Pages = {41 - 58},
Publisher = {IAP Information Age Publishing},
Title = {Environmental displacement, English learners, and value creation: Considering Daisaku Ikeda in the East-West ecology of education.},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2011-30521-003&site=ehost-live&scope=site},
Year = {2012},
}


@article{2011-23263-00920111001,
Abstract = {In this study, we employed structural equation modeling to test the degree to which racism-related stress, acculturative stress, and bicultural self-efficacy were predictive of mental health in a predominantly community-based sample of 367 Asian American adults. We also tested whether bicultural self-efficacy moderated the relationship between acculturative stress and mental health. Finally, we examined whether generational status moderated the impact of racial and cultural predictors of mental health by testing our model across immigrant and U.S.-born samples. Results indicated that our hypothesized structural model represented a good fit to the total sample data. While racism-related stress, acculturative stress, and bicultural self-efficacy were significant predictors of mental health in the total sample analyses, our generational analyses revealed a differential predictive pattern across generational status. Finally, we found that the buffering effect of bicultural self-efficacy on the relationship between acculturative stress and mental health was significant for U.S.-born individuals only. Implications for research and service delivery are explored. (PsycINFO Database Record (c) 2015 APA, all rights reserved). (journal abstract)},
Author = {Miller, Matthew J. and  Yang, Minji and  Farrell, Jerome A. and  Lin, Li‐Ling},
