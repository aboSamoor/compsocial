Journal = {Psychology & Marketing},
Keywords = {cultural identity, salience, apparel, brand choices, Hispanic consumers, acculturation, social identity, Acculturation, Brand Preferences, Consumer Behavior, Ethnic Identity, Social Identity, Latinos/Latinas},
Number = {3},
Pages = {263 - 284},
Title = {Social identity salience: Effects on identity-based brand choices of Hispanic consumers.},
URL = {vchattaraman@auburn.edu},
Volume = {27},
Year = {2010},
}


@inbook{2009-22382-00620100101,
Abstract = {While strides have been made in providing underrepresented groups with more opportunities, there remain challenges prohibiting individuals from partaking in certain opportunities. This chapter identifies the limitations affecting one's ability to make the desired contributions. Some may be related to ethnic and cultural factors; others may reflect the sociocultural context. One component of success is an awareness of what one brings to a situation (beliefs, values, ethnicity, gender, etc.), as well as the ability to assess a situation. With increased awareness of the factors limiting opportunities, individuals may be able to more effectively employ strategies for success. Another intent of this chapter is to help individuals anticipate specific circumstances and handle the challenges that arise because of ethnic or cultural differences. Devoting attention to one's interactional style is important in developing appropriate coping strategies. This chapter also illuminates the challenges that Asian Americans face due to their bicultural worlds and addresses the importance of providing opportunities that enable people to achieve mutually shared goals. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (create)},
