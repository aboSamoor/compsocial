@article{2011-02638-01020070901,
Abstract = {Empirical research has shown that exposure to race-related stressors in the military by Asian American Pacific Islander Vietnam veterans, now reliably measurable, contributes uniquely and significantly to post-traumatic stress disorder (PTSD) symptoms and generalized psychiatric distress; moreover, studies reveal that adverse race-related events can meet Diagnostic and Statistical Manual of Mental Disorders-IV criteria for a PTSD diagnosis. Competence in treating PTSD or general psychiatric distress requires understanding the types of, and effects of, adverse race-related events experienced by ethnic minority veterans. Case studies highlight two types of race-related stressors—“bicultural identification and conflict” and “racial stigmatization”—which placed the veteran at greater risk of death and reduced cohesion with fellow service members. The studies demonstrate the presence of race-related stressors in one or more of the four major types of war zone stressors: traditional combat, atrocities-abusive violence, perceived threat, and malevolent environment. These case studies supplement the empirical findings on racerelated stressors and PTSD, enlarging the clinician’s understanding of this unique type of mental health risk factor. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Loo, Chalsa M. and  Lim, Brian R. and  Koff, Gabriel and  Morton, Robert K. and  Kiang, Peter N. C.},
ISSN = {0026-4075},
Journal = {Military Medicine},
Keywords = {ethnic related stressors, war zone, Asian Americans, Vietnam veterans, racial stigmatization, posttraumatic stress disorder, Military Veterans, Posttraumatic Stress Disorder, Racial and Ethnic Relations, Stigma, Asians, War},
Number = {9},
Pages = {968 - 971},
Title = {Ethnic-Related Stressors in the War Zone: Case Studies of Asian American Vietnam Veterans.},
Volume = {172},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2011-02638-010&site=ehost-live&scope=site},
Year = {2007},
}


@article{2007-15418-00620070901,
Abstract = {The offenses of Virginia Tech senior Seung-Hui Cho that ended in the death of thirty-three people and the wounding of numerous others was enough to disorient anyone; when these crimes were considered in conjunction with his Korean ethnicity, the world was not only stunned but confused. The majority of Koreans, immigrants and foreigners alike, expressed shame and guilt that one of their own had caused such monumental damage. As a bilingual and bicultural Korean American licensed counselor, the current author was summoned by Virginia Tech to conduct counseling, education, and outreach mostly with Asian students on campus in the aftermath of the shootings. While the violence could have been perpetrated by an adolescent of any racial or ethnic background, it is important to examine the incident using a multicultural lens and to acknowledge the cultural context in which the incident took place. The aim of this article is not to cast blame but to examine salient cultural factors that may have contributed to the incident and to explore preventative interventions from a multicultural perspective. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Kim, Josephine M.},
ISSN = {0017-8055, 1943-5045},
Journal = {Harvard Educational Review},
Keywords = {Korean-Americans, caring, tolerance, multicultural perspective, school violence, cultural factors, School Violence, Sociocultural Factors, South Asian Cultural Groups, Multiculturalism, Empathy, Tolerance},
Number = {3},
Pages = {354 - 359},
Title = {A push for caring imposition against passive tolerance: Reflections on the Virginia Tech massacre through a multicultural lens.},
Volume = {77},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2007-15418-006&site=ehost-live&scope=site},
Year = {2007},
}


@article{2007-13756-00820070901,
Abstract = {This article reports on the experiences of a group of deaf and hard-of-hearing alumni of Griffith University in south-east Queensland, Australia. Participants completed a survey answering questions about their communication patterns and preferences, working lives, career barriers or difficulties anticipated and encountered, and workplace accommodations used or sought. Results revealed a range of career barriers and workplace difficulties encountered by these participants, as well as solutions found and strategies used by them. Differences in employment sector, job-search activities, difficult workplace situations, and use of accommodations were noted between 2 groups: those who communicated primarily in Australian Sign Language and considered themselves to have a Deaf or bicultural identity and those who communicated primarily in spoken English and considered themselves to have a hearing identity. Implications for university services supporting deaf and hard-of-hearing students are outlined, and suggestions for further research are made. (PsycINFO Database Record (c) 2013 APA, all rights reserved). (journal abstract)},
Author = {Punch, Renée and  Hyde, Merv and  Power, Des},
ISSN = {1081-4159, 1465-7325},
Journal = {Journal of Deaf Studies and Deaf Education},
