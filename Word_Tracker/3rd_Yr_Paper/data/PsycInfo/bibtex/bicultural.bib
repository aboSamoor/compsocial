Year = {2009},
}


@article{2010-00248-01820090101,
Abstract = {Little is known about what constitutes a good death, although palliative care is broadly aimed at enabling the good death. Good deaths could vary considerably across populations. A two stage ethnographic study was conducted in rural Canada, involving English-speaking Albertans and French-Canadians in Quebec, to establish a conceptual understanding of the good death from a rural perspective. This study identified four common bicultural elements, as well as a conceptual model focusing on quality of life through four dimensions: physical, spiritual, social, and emotional/psychological. Rurality was identified as a dominant consideration for further study. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
