Journal = {Family Process},
Keywords = {Autism, Culture, Latino, Parent education, Promotora, autismo, cultural, latinos, capacitación de los padres, promotora, 自闭症, 文化, 拉丁裔, 家长教育, promotora de salud, No terms assigned},
Title = {Parents taking action: A psycho‐educational intervention for latino parents of children with autism spectrum disorder.},
