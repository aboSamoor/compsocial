Title = {The efficacy of fingerspell coding and visual imaging techniques in improving the spelling proficiency of deaf signing elementary-phase children: A South African case study.},
URL = {vanstadena.hum@ufs.ac.za},
Volume = {22},
Year = {2010},
}


