ISSN = {0739-9863, 1552-6364},
Journal = {Hispanic Journal of Behavioral Sciences},
Keywords = {transnational, social order, Mexicanness, identity, digital communication, Communication, Ethnic Identity, Social Identity, Nationalism, Social Networks},
