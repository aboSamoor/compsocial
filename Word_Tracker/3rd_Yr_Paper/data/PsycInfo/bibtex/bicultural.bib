Year = {1999},
}


@inbook{1998-06344-00419990101,
Abstract = {Explores traditional and contemporary sex roles of Indian women. This article emphasizes the renewing power of the feminine—a creative, healing balance that arises as traditional and contemporary strengths are brought together. Tribal diversity and predominantly egalitarian structural similarities are affirmed in this work through reviews of ethnographic studies addressing the roles of Native American women (NAWs) prior to European contact. The conventional and alternative roles of NAWs in traditional times are examined with an eye toward the spiritual source of the NAWs' strength. Studies outlining the emotional and spiritual costs of contemporary NAWs living bicultural lifestyles, especially those pursuing advanced educational training, highlight the continued use of traditional coping mechanisms. Finally, the current movement toward retraditionalization of roles of NAWs as caretakers and transmitters of cultural knowledge is posited as an effective means of overcoming problems and achieving Native American self-determination. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
