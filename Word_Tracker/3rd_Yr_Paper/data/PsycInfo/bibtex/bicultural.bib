@article{2013-02133-00520130101,
Abstract = {Reviews the book, With Respect to the Japanese: Going to Work in Japan2nd Edition by J. C. Condon and T. Masumoto (2011). This new edition is not a mere revision of the earlier text, being thematically and structurally different enough that reading both editions is recommended. The subtitle of the first edition was 'A Guide for Americans' in contradistinction to the new version's 'Going to Work in Japan', a difference suggestive of both a broadening and a narrowing of scope. Broadening in that the new edition transcends the bicultural, contrastive theme of its predecessor, and narrowing in that its target readership is more sharply defined as prospective employees. There is throughout the text a refreshing lack of academic jargon, and the material is presented in elegant prose that describes complex concepts with admirable clarity. Condon and Masumoto have wisely attempted to provide only the most general guidelines for working in Japan. And in spite of its subtitle's implication that this is a text for prospective workers, anyone with an interest in Japan would benefit from this book, if only as an indispensable introductory guide while waiting for the authors to produce the authoritative, comprehensive text on Japan's distinct yet increasingly fissiparous culture. (PsycINFO Database Record (c) 2013 APA, all rights reserved)},
Author = {Harris, Richard},
ISSN = {0147-1767},
Journal = {International Journal of Intercultural Relations},
Keywords = {Japan, prospective employees, working guidelines, Japanese culture, Cross Cultural Communication, Culture (Anthropological), Foreign Workers, Sociocultural Factors},
Number = {1},
Pages = {127 - 129},
Title = {Review of With respect to the Japanese: Going to work in Japan.},
URL = {richardh@mecl.chukyo-u.ac.jp},
Volume = {37},
Year = {2013},
}


@article{2012-33261-00820130101,
Abstract = {Biculturalism (having two cultures) is a growing social phenomenon that has received considerable attention in psychology in the last decade; however, the issue of what impact (if any) biculturalism has on individuals’ adjustment remains empirically unclear. To answer this question, we conducted a meta-analysis that included 83 studies, 322 rs, and 23,197 participants. Results based on the random-effects approach show a significant, strong, and positive association between biculturalism and adjustment (both psychological and sociocultural). This biculturalism-adjustment link is stronger than the association between having one culture (dominant or heritage) and adjustment. Thus, our results clearly invalidate early sociological accounts of this phenomenon, which portrayed bicultural individuals as “marginal” and stumped between two worlds. Analyses also indicate that the association between biculturalism and adjustment is moderated by how acculturation is measured, the adjustment domain, and sample characteristics. (PsycINFO Database Record (c) 2015 APA, all rights reserved). (journal abstract)},
Author = {Nguyen, Angela-MinhTu D. and  Benet-Martínez, Verónica},
ISSN = {0022-0221, 1552-5422},
Journal = {Journal of Cross-Cultural Psychology},
Keywords = {biculturalism, adjustment, acculturation, social phenomenon, Acculturation, Social Processes, Multiculturalism, Adjustment},
Number = {1},
Pages = {122 - 159},
Title = {Biculturalism and adjustment: A meta-analysis.},
URL = {amnguyen@fullerton.edu},
Volume = {44},
Year = {2013},
}


@article{2012-33261-00720130101,
Abstract = {Prior research has documented cultural dimensions that broadly characterize between-culture variations in Western and East Asian societies and that bicultural individuals can flexibly change their behaviors in response to different cultural contexts. In this article, we studied cultural differences and behavioral switching in the context of the fast emerging, naturally occurring online social networking, using both self-report measures and content analyses of online activities on two highly popular platforms, Facebook and Renren (the “Facebook of China”). Results showed that while Renren and Facebook are two technically similar platforms, the Renren culture is perceived as more collectivistic than the Facebook culture. Furthermore, we presented evidence for the first time that users who are members of both online cultures flexibly switch and adapt their in-group sharing behaviors in response to the online community in which they are: They perform more benevolent in-group sharing when they participate in the Renren community and less so when they participate in the Facebook community. We discussed both the theoretical and methodological implications of the current research. (PsycINFO Database Record (c) 2013 APA, all rights reserved). (journal abstract)},
Author = {Qiu, Lin and  Lin, Han and  Leung, Angela K.-y.},
ISSN = {0022-0221, 1552-5422},
Journal = {Journal of Cross-Cultural Psychology},
Keywords = {cultural differences, ingroup sharing behavior, behavioral switching, social networking sites, Cross Cultural Differences, Intergroup Dynamics, Sharing (Social Behavior), Online Social Networks},
Number = {1},
Pages = {106 - 121},
Title = {Cultural differences and switching of in-group sharing behavior between an American (Facebook) and a Chinese (Renren) social networking site.},
URL = {linqiu@ntu.edu.sg},
Volume = {44},
Year = {2013},
