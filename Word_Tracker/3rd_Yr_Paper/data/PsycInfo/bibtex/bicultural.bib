@inbook{1999-02576-00619980101,
Abstract = {In this chapter, Ho underlines that Yu Shun has been the model man to more people than any other since the dawn of civilization, although many people today have never heard of him. Ho contrasts Yu Shun's selfless attitude toward his parents in the face of their cruel treatment of him with Western stories such as Oedipus Rex in which the "hero" kills his father. Whereas father–son relationships in Chinese culture are expected to result in the "psychological capitulation" of the son, the Western hero tries to free himself from the emotional fetters imposed upon him by his parents, and then strikes out on his own. But Ho, whose bicultural background sensitizes him to both the power of Confucian moralism and the liberating messages of Western psychology, is not an admirer of the psychological legacy of the Legend of Yu Shun and of the countless other Chinese stories containing similar messages. To Ho, the self-sacrificial attitude modeled by Yu Shun frequently leads to self-destructive behavior, authoritarian moralism, and the strict control of one's "improper" impulses and thoughts. For the Chinese family therapist, this means that genuine harmonization of family relationships requires recognizing and working through ambivalent feelings toward one's family members. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (book)},
Author = {Ho, David F. Y. and  Gielen, Uwe P. and  Comunian, Anna Laura and  Gielen, Uwe P. (Ed) and  Comunian, Anna Laura (Ed)},
Booktitle = {The family and family therapy in international perspective.},
ISBN = {88-8190-033-5},
Pages = {134 - 149},
Publisher = {Edizioni Lint Trieste},
Title = {Filial piety and filicide in Chinese family relationships: The legend of Shun and other stories.},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=1999-02576-006&site=ehost-live&scope=site},
Year = {1998},
}


@article{1998-10735-00219980101,
Abstract = {The purpose of this study was to examine the effectiveness of a nonbiased computerized language screening test, the Kidtalk Interactive Diagnostic Test of Aptitude for Language Knowledge (KIDTALK), in differentiating children on the basis of diagnostic category but not ethnic/linguistic background. KIDTALK was designed to assess children's aptitude for acquiring linguistic knowledge of an invented language. Participants were 29 Anglo/English-speaking (A/E) and 31 Hispanic/Spanish-speaking (H/S) 7- and 8-year-old children. 21 were language-learning disabled (LLD), and 39 were non-language-learning disabled (NLLD). Results showed a statistically significant difference between the KIDTALK scores of the LLD and NLLD groups, no difference between the A/E and H/S groups, and no interaction effect. Discussion emphasizes that the computer can be an effective tool for identifying LLD in bicultural populations. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
