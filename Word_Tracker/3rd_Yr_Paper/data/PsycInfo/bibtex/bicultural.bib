@article{2013-03245-00220120101,
Abstract = {Developing health messages for bicultural audiences can present challenges because of potentially competing views about an issue that might be tied to each culture. To help illuminate what such beliefs might be with respect to smoking, a qualitative study was conducted with Mexican American adolescents in two U.S.-Mexico border communities. Adapting a consumer-behavior research method, we asked participants to construct collages representing their views of Mexican and American cultures as well as the overlap between the cultures. Participants then sorted concepts related to identity and tobacco within the cultural domains. Findings involved shifting cultural identities and the association of both tobacco promotion and prevention with American culture. Results also suggest that “making it” in America while remaining true to Mexican heritage is viewed as a much-admired outcome. It is suggested that framing health behaviors as consistent with this aspirational image is a promising health-promotion approach. (PsycINFO Database Record (c) 2013 APA, all rights reserved). (journal abstract)},
Author = {Comello, Maria Leonora G. and  Kelly, Kathleen J.},
ISSN = {1540-4153, 1938-8993},
Journal = {Hispanic Health Care International},
Keywords = {biculturalism, health behaviors, smoking, identity, prevention, Mexican Americans, Ethnic Identity, Health Behavior, Prevention, Tobacco Smoking, Multiculturalism, Adolescent Development, Mexican Americans},
Number = {3},
