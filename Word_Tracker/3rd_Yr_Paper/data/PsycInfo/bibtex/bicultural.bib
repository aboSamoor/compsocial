Volume = {34},
Year = {2010},
}


@article{2011-02966-00120101001,
Abstract = {Following immigration emerges a need for integrating multiple identities into the sell. However, researchers disagree as to which strategy favors well-being. A high well-being is associated to perceiving less conflict and distance between the two cultures according to the bicultural identity theory, and to high identification to both cultures according to the acculturation strategies theory. The cognitive-developmental model, elaborated recently, predicts that perceiving higher coherence between both cultures will be related to higher well-being. The predictive values of these theories have been assessed with bicultural individuals. Regressions and contrasts tests support more strongly the cognitive-developmental model. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
