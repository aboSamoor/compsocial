Year = {2001},
}


@inbook{2008-00399-02920010101,
Abstract = {Multicultural qualitative research in counseling attempts to ground research in the lived experiences of those whose lives we investigate. In this chapter, we hope to introduce you to a discovery process in which mother wit and wisdom play a huge role, where personal experience is considered very good evidence, and distant statistics are certainly not as important as the actual experience of a sober person. Writing a chapter about qualitative research in multicultural counseling is a tricky task. It is, in a sense, a bicultural endeavor. To understand more fully how qualitative research can complement traditional counseling research methods, we provide an examination of various paradigms for qualitative research that can benefit multicultural counseling. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
