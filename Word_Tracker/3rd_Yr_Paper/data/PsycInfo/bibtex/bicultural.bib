Year = {2002},
}


@book{2002-00740-00020020101,
Abstract = {This book is an ethnographic account of a revolutionary Indigenous self-determination movement that began in 1966 with the Rough Rock Demonstration School. Called Diné Bi'ólta', The People's School, in recognition of its status as the first American Indian community-controlled school, Rough Rock was the first to teach in the Native language and to produce a body of children's literature by and about Navajo people. These innovations have positioned the school as a leader in American Indian and bilingual/bicultural education and have enabled school participants to wield considerable influence on national policy. Unlike most accounts of Indigenous schooling, this book involves the active participation of Navajo community members. Their oral testimony and that of other leaders in Indigenous/Navajo education frame and texture the account. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (cover)},
Author = {McCarty, Teresa L.},
