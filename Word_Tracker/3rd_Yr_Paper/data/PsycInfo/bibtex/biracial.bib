Number = {5},
Pages = {570 - 571},
Title = {Elder self-neglect: Medical emergency or marker of extreme vulnerability?},
URL = {thomas.gill@yale.edu},
Volume = {302},
Year = {2009},
}


@article{2006-21406-00420061001,
Abstract = {Poverty can shorten the average person's life span by up to 10 years. Socioeconomic status (SES) and its relation to stroke mortality has been extensively investigated, and lower SES has been associated with a higher mortality after stroke in Europe and the United States. In addition, poorer SES has been associated with a higher prevalence of stroke risk factors and lower poststroke quality of life. Within our large, biracial population, we found that 39% of the racial disparity in stroke incidence for blacks was associated with poorer SES. Stroke incidence increased as community poverty increased, suggesting a dose-response. Although SES may explain 39% of the racial disparity in stroke incidence, the remainder is still unexplained. Unfortunately, we cannot explain the contribution of hypertension, smoking, and other traditional risk factors to the excess risk for blacks, because we do not have reliable estimates of risk factor prevalences for the population subgroups, as we do for census-based socioeconomic variables. There is likely significant overlap between the prevalence of risk factors and SES, and there may also be differences in medical control of risk factors associated with SES. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Kleindorfer, Dawn O. and  Lindsell, Christopher and  Broderick, Joseph and  Flaherty, Matthew L. and  Woo, Daniel and  Alwell, Kathleen and  Moomaw, Charles J. and  Ewing, Irene and  Schneider, Alexander and  Kissela, Brett M.},
ISSN = {0364-5134, 1531-8249},
Journal = {Annals of Neurology},
Keywords = {stroke risk factors, risk factor prevalences, racial disparity, community poverty, socioeconomic status, blacks, hypertension, smoking, Cerebrovascular Accidents, Life Span, Quality of Life, Risk Factors, Socioeconomic Status, Blacks, Hypertension, Tobacco Smoking},
Number = {4},
Pages = {480 - 484},
Title = {Impact of Socioeconomic Status on Stroke Incidence: A Population-Based Study.},
URL = {dawn.kleindorfer@uc.edu},
Volume = {60},
Year = {2006},
}


@article{2008-15042-01020081001,
Abstract = {We have been collecting data from healthy, young-adult blacks in a study relating posttraumatic stress disorder (PTSD) to nocturnal blood pressure and would like to add to the discussion of sleep paralysis (SP): findings from measures of recent and lifetime SP, self-reported sleep duration and continuity, and symptoms of PTSD and depression. Our sample participants self identified as African American, African. West Indian/ Caribbean and biracial, and ranged from 18-34 years old. The prevalence of SP among blacks in our sample (40%) was comparable to that reported in other study. The similarity of sleep duration in the 2 groups suggests that sleep deprivation is not a primary determinant of SP. We look forward to presenting more definitive analyses of these relationships as our study progresses. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
