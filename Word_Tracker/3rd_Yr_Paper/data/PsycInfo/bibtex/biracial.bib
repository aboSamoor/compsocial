Year = {2001},
}


@article{2000-16298-00720001001,
Abstract = {Investigated the relationship of pessimistic attributional style (specifically, stable attributions for negative events) and socioeconomic status (SES) to cardiovascular and catecholamine profiles in a biracial (Black vs White) sample of 37 postmenopausal women (aged 39–64 yrs) not taking hormone replacement therapy. Blood pressure (BP) variation in response to the demands of daily life was assessed by 24-hr ambulatory monitoring on a typical workday. Ss were classified into groups by stable pessimistic attributions (high vs low pessimism) and by SES (high vs low). Significant SES × pessimism interactions were found. Low SES/high pessimism women demonstrated higher systolic BP (SBP) during the day, evening, and sleep periods of 24-hr ambulatory monitoring compared with the other three groups. A greater proportion of this group was in the hypertensive range compared with the other groups. Low SES/high pessimism women also reported reduced available social support compared with the other three groups. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
