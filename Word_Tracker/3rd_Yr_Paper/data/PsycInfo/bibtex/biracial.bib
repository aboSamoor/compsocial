Keywords = {marijuana use, cigarette smoking, high risk African American youth, nicotine, gateway drugs, At Risk Populations, Blacks, Marijuana, Nicotine, Tobacco Smoking},
Number = {6},
Pages = {782 - 791},
Title = {Does marijuana use serve as a gateway to cigarette use for high-risk African-American youth?},
URL = {mvaughn9@slu.edu},
Volume = {34},
Year = {2008},
}


@inbook{2008-05087-02620080101,
Abstract = {Relational Dialectics Theory (RDT) is a theory of the meaning-making between relationship parties that emerges from the interplay of competing discourses. "Discourses" are systems of meaning that are uttered whenever we make intelligible utterances aloud with others (or in our heads when we hold internal conversations). In its broadest sense, a discourse is a cultural system of meaning that circulates among a group's members and which makes our talk sensical. For example, in the United States the discourse of individualism helps us to understand and value an utterance such as, "I need to find myself first before I commit to a serious relationship with another person." Systems of meaning can be more localized, as well. For example, when partners refer to one another with a favorite idiom, such as "chickadee," they might be invoking a core meaning system about themselves as a biracial couple. Whenever we communicate, we are invoking--often indirectly and by implication--multiple systems of meaning. These discourses often compete, oppose, and struggle with one another, however. We often hear these discursive struggles when people talk. Consider this statement about dating by a college student: "Well, I'm kinda, like, seeing him, but we're not, ya know, serious." This utterance displays a struggle in meaning. It draws on a discourse of connection or closeness ("I'm seeing him"), yet it also draws on a discourse of autonomy or distance ("We're not serious"). Relational Dialectics Theory focuses on the struggles in meaning--the discursive tensions--that frequent interpersonal communication. However, the position of the theory is not that competing discourses are negative. Instead, they are the heart of the meaning-making enterprise. The central claim of RDT is that such discursive tensions are both inevitable and necessary. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
Author = {Baxter, Leslie A. and  Braithwaite, Dawn O. and  Baxter, Leslie A. and  Braithewaite, Dawn O. and  Baxter, Leslie A. (Ed) and  Braithewaite, Dawn O. (Ed)},
Booktitle = {Engaging theories in interpersonal communication: Multiple perspectives.},
ISBN = {978-1-4129-3852-5, 978-1-4129-3851-8},
Pages = {349 - 361},
Publisher = {Sage Publications, Inc},
Title = {Relational dialectics theory.},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2008-05087-026&site=ehost-live&scope=site},
Year = {2008},
}


@article{2008-04819-00120080101,
Abstract = {Reviews the book, Children and separation: Socio-genealogical connectedness perspective by Kwame Owusu-Bempah (see record [rid]2007-03300-000[/rid]). Adopted children, especially those in biracial adoption situations, may grow up unconnected. This is an unfortunate situation, and this book is a laudable attempt to correct this oversight. As such, the topic of this book is timely, and it will be useful for many families and the social workers, counselors, and psychologists who work with these families. The main concern of the book is the sociogenealogical connectedness of children. Separation of children from their biological families is an issue that needs to be addressed by additional research, and the book makes a strong case for that. It also integrates sociogenealogical connectedness with accepted theories of development and provides both parents and mental health professionals with valuable ideas. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
