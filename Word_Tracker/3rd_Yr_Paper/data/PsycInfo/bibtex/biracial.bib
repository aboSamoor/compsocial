Year = {2004},
}


@inbook{2004-12475-01220040101,
Abstract = {The author shares not only her experiences growing up as a biracial lesbian Samoan/Mexican American, but also how those experiences led her to become a social activist. Being queer, biracial, and a feminist (or as the author identifies: a woman who actively works toward "letting people have it" for their multiple levels of ignorance and hate) in a world that silences, threatens, and incarcerates those who are different, the author felt the three strikes initiative was perfect for describing her place. Most know the three strikes initiative as a legal way to unjustly imprison poor men and women of color for life. In the act of disrupting this horrifying legal practice of three strikes, the author has chosen to present each of these strikes in a different way: (1) Her strike of pride; she challenges family loyalties for being Mexican and Samoan. (2) Her strike of resistance; she challenges sexist and homophobic practices as a woman in sexist Chicano organizations. (3) Her strike of liberation; she challenges the heterosexual life that her family members, friends, and fellow activists have expected her to live. It is with each of these strikes that she now challenges multiple notions of ignorance, disrupts multiple levels of comfort, and creates a safe and consistent space. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
Author = {Misa, Cristina M. and  Kumashiro, Kevin K. and  Kumashiro, Kevin K. (Ed)},
Booktitle = {Restoried selves: Autobiographies of Queer Asian/Pacific American activists.},
ISBN = {1-56023-462-8},
Pages = {87 - 91},
Publisher = {Harrington Park Press/The Haworth Press},
Title = {Three strikes, I'M OUT! A Chicana Samoan dyke's tale.},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2004-12475-012&site=ehost-live&scope=site},
Year = {2004},
}


