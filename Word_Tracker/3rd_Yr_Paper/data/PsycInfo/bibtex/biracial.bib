ISSN = {0306-4603},
Journal = {Addictive Behaviors},
Keywords = {tobacco smoking dosage, body mass index, body weight, weight change, adolescent development, duration of smoking exposure, ethnic differences, gender differences, Caucasians, African Americans, Adolescent Development, Body Weight, Human Sex Differences, Racial and Ethnic Differences, Tobacco Smoking, Blacks, Body Mass Index, Whites},
Number = {3},
