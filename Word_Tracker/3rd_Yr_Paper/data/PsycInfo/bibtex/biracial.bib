@article{1984-25588-00119841201,
Abstract = {Examined the self-concepts of ability, general self-concepts, levels of externality, and the attitudes toward physical characteristics, neighborhoods, and schools of 98 8–18 yr old Black students. Most of the Ss had upper middle class and highly educated parents. Ss were administered the Rosenberg Self-Esteem Scale, the Nowicki-Strickland Locus of Control Scale, a self-concept of ability scale, a racial attitude scale, and a 45-item questionnaire, developed by the investigator to measure, among other factors, attitudes toward both Whites and Blacks. Results show that the predominantly White suburban communities in which the Ss were socialized had not prevented them from developing positive attitudes toward themselves, their communities, and their schools. Ss were biracial in their attitudes—they had positive attitudes toward both Blacks and Whites—although they were slightly more positive toward Blacks than Whites. Findings suggest that Black children socialized within predominantly White suburban communities are likely to become highly attitudinally assimilated into White society and that this kind of assimilation may have complex effects on their racial attitudes toward Blacks and their levels of ethnocentrism. As attitudinal assimilation increased, Ss became increasingly more positive toward their schools and neighborhoods and more positive toward Whites, but less positive toward Blacks. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Banks, James A.},
ISSN = {0022-2984},
Journal = {Journal of Negro Education},
