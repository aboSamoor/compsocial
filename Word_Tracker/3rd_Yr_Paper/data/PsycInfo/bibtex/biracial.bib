Volume = {34},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2006-08168-009&site=ehost-live&scope=site},
Year = {2006},
}


@article{2000-05893-00120000701,
Abstract = {Employed a case study of 102 Black college students at a predominantly White liberal arts college  to question continuing assumptions in sociological research that Blacks represent social, cultural, and economic homogeneity and that intraracial relations are close and without conflict. The authors examined the extent to which Black students feel close to other black students on campus, rely on one another for need satisfaction and social interaction, and share similar values, experiences, and attitudes. The authors find that the odds of closeness are significantly reduced for biracial students, for Black students from low SES backgrounds, and for Black students from predominantly White backgrounds. Among biracial students and those from predominantly White environments, social distance reflected their overall distance from the larger Black community and is a function of their lack of racial awareness. The social distance of low SES students appeared specific to the community of Black students within the college and is not expressed toward the larger black community. Moreover, feelings of distance appeared to be driven by perceived social, cultural, and economic differences from other Black students. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Smith, Sandra S. and  Moore, Mignon R.},
ISSN = {0002-9602},
Journal = {American Journal of Sociology},
Keywords = {intraracial diversity & relations among African-Americans & closeness among Black students at a predominantly White university, college students, Blacks, Racial and Ethnic Relations, School Environment, Social Interaction, Whites, Diversity},
Number = {1},
Pages = {1 - 39},
Title = {Intraracial diversity and relations among African-American: Closeness among black students at a predominantly white university.},
