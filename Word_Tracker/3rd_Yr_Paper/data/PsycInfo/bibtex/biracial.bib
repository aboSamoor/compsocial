Title = {Negotiating the color line: The gendered process of racial identity construction among Black/White biracial women.},
URL = {rockquem@bc.edu},
Volume = {16},
Year = {2002},
}


@article{2003-04354-00120020601,
Abstract = {Examined racial identity construction among biracial individuals. 177 biracial college students (aged 15-58 yrs) completed surveys regarding biracial identity, racial composition of social networks, negative experiences, affinities toward Blacks and Whites as groups, self-reported skin color, and perceptions of how others view them. Rockquemore's taxonomy of racial identity options (1999) was used as a framework to determine the Ss' understandings of their racial identities. Results show a great deal of variation in the racial composition of both preadult and adult social networks. Ss' contextual experiences of race varied widely. On average, Ss experienced fairly high levels of negative treatment from both Blacks and Whites. There was significant variation in biracial individuals' feelings of closeness toward Blacks and Whites as groups. Significant variation was observed concerning both the self-perceived skin color index and socially perceived appearance. Findings support a multidimensional view of racial identity by illustrating that biracial people make various identity choices, albeit choices that are differentially available due to an individual's structural location. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Rockquemore, Kerry Ann and  Brunsma, David L.},
ISSN = {0038-0253, 1533-8525},
Journal = {The Sociological Quarterly},
Keywords = {racial identity, identity construction, biracial identity, social networks, experiences, Blacks, Whites, skin color, self perception, Blacks, Ethnic Identity, Interracial Offspring, Self Perception, Whites, Experiences (Events), Skin (Anatomy), Social Support},
Number = {3},
Pages = {335 - 356},
Title = {Socially embedded identities: Theories, typologies, and processes of racial identity among Black/White biracials.},
URL = {brunsmad@email.uah.edu},
Volume = {43},
Year = {2002},
}


@article{2002-02715-00320020101,
Abstract = {Discusses provision of social services to biracial individuals. A biracial individual may assume a multiplicity of identities, including African-, Asian-, Latino-, and Native-American when negotiating with large institutions, including those providing social services. As an alternative to traditional, static views of race, the identity-across-the-lifespan model provides a more comprehensive paradigm for biracial clients. In the aftermath clients will be rendered visible by such an identity model, which prevails less on the basis of race and more on the basis of lifetime experience. It is concluded that biracial clients require the option, at least, to identify themselves rather than be identified by superficial racial characteristics. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
