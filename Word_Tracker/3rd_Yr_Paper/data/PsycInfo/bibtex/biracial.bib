Journal = {The Journal of Head Trauma Rehabilitation},
Keywords = {community integration, traumatic brain injury, preinjury differences, racial bias, Whites, African Americans, TBI rehabilitation, outcome measurement bias, Neuropsychological Rehabilitation, Racial and Ethnic Differences, Social Integration, Traumatic Brain Injury, Blacks, Individual Differences, Test Bias, Whites},
Number = {2},
Pages = {158 - 172},
Title = {Community Outcomes Following Traumatic Brain Injury: Impact of Race and Preinjury Status.},
