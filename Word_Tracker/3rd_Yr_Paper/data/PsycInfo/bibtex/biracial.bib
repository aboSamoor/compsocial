ISSN = {0038-4941, 1540-6237},
Journal = {Social Science Quarterly},
Keywords = {Black density, White crossover vote, social interaction theory, racial interaction, White racial tolerance, Racial and Ethnic Relations, Social Density, Social Interaction, Theories, Voting Behavior, Blacks, Tolerance, Whites},
Number = {3},
