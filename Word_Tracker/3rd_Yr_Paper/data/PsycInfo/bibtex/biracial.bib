Journal = {Journal of Sandplay Therapy},
Keywords = {treatment, depression, symptoms, physical violence, Major Depression, Self Concept, Symptoms, Treatment, Adolescent Development},
Number = {2},
Pages = {111 - 131},
Title = {The case of Clare: The emergence of the self in a six-year-old girl.},
URL = {sbherrmann@comcast.net},
Volume = {18},
Year = {2009},
}


@inbook{2009-11006-00420090101,
Abstract = {Over the last decade, a number of biracial persons have achieved positions of prominence in the United States. Tiger Woods, son of an African American father and an Asian mother, is one of the world's best and most celebrated golfers. Halle Berry, Academy Award-winning actress, is the daughter of an African American father and a white mother. Norah Jones, celebrated musician, is the daughter of a white mother and an Indian father, while Barack Obama, the current president of the United States, is the son of a white mother from Kansas and a black father from Kenya. While much attention has been paid to these accomplished individuals, much less attention has been paid to the interracial marriages that produced them. Yet, their very presence is a reflection of the extent to which interracial marriage is a rapidly growing phenomenon in the United States. What are the implications of this increasing rate of intermarriage as a reflection of and, simultaneously, a catalyst for social change? Given our society's historic reliance on rigid classifications, especially in regard to race, do these marriages result in identity conflicts for the partners, both as individuals and as a couple? To what extent do interracial couples find family and societal support for their choice of partner? To what extent do they struggle with negative responses to the breaking of what some have labeled "the last taboo"? And what are the implications for partner and couple well-being? In this chapter, we explore these questions in the context of what is known and what is not known about the factors associated with the growing phenomenon of interracial marriage. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
Author = {Steil, Janice M. and  Rothman, Allison M. and  Harris, Lisa E. and  Kim, Gideon S. and  Steil, Justin P. and  Chin, Jean Lau and  Chin, Jean Lau (Ed)},
Booktitle = {Diversity in mind and in action, Vol 1: Multiple faces of identity.},
ISBN = {978-0-313-34709-2, 978-0-313-34710-8},
Pages = {47 - 60},
Publisher = {Praeger/ABC-CLIO},
Series = {Praeger perspectives: Race and ethnicity in psychology},
Title = {Interracial marriage: Current perspectives and unanswered challenges.},
