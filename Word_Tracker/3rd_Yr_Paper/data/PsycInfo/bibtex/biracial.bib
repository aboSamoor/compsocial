Title = {A prospective evaluation of the relationships between smoking dosage and body mass index in an adolescent, biracial cohort.},
URL = {bklesges@cc.memphis.edu},
Volume = {28},
Year = {2003},
}


@article{2003-99874-00320030201,
Abstract = {Conventional norms that test presumably normal elderly individuals at one point in time may include preclinical cases of dementia and therefore may be less sensitive to the presence of dementia. A sample of presumably normal African American and White rural community older adults were retested after approximately 4 years to develop robust norms for the Mini Mental State Examination, Mattis Dementia Rating Scale Fuld Object Memory Evaluation, WAIS-R Vocabulary and Block Design, Wechsler Memory Scale-Revised Logical Memory and Visual Reproduction, Raven's Colored Progressive Matrices, and Clock Drawing Test. 94 out of the original 133 participants were located and agreed to be retested. 12 of the participants retested showed significant decline on testing relative to their own baseline and were dropped from the recalculated norms. Participants who declined on testing tended to be older, less educated, had lower WAIS-R scores on Vocabulary and Block Design combined, had poorer IADLs and were less socially active. There was no difference in physical health status or level of depression. Recalculated group means showed little change when the participants who declined had been removed, but this left very few participants at the extremes of age (>85 years) and education (<4 years). (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
