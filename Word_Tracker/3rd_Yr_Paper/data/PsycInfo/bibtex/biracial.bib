Title = {The APOE-E4 allele and the risk of functional decline in a community sample of African American and White older adults.},
URL = {blaze001@mc.duke.edu},
Volume = {56A},
Year = {2001},
}


@article{2001-05892-00220010901,
Abstract = {Explored whether a racial/ethnic group cross-over effect is emerging in the use and sales of methamphetamine (MTH) or "crank," particularly by African American American gang members. Data on 1,042 juvenile and adult offenders who were self-reported gang members were collected as part of Project GANGMILL from the National Gang Crime Research Center. The research also examined use and sales of crack cocaine (CC). Survey results are summarized for: regional variations in MTH use, gender differences, racial differences (African American, Hispanic/Latino, White, Asian/Chinese, Native American Indian, biracial), and gang alliance factors. The results show a cross-over effect for methamphetamine use and sales, but indicate that some preference patterns continued when race or ethnic background was controlled. While the results show that African-American gang members were less likely to use MTH than other racial categories, such gang members were not more likely to prefer CC instead or to be involved in CC sales. The strongest evidence of a cross-over effect emerged in the evidence for ample competition across ethnic groups for sales of MTH through their gang organization. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
