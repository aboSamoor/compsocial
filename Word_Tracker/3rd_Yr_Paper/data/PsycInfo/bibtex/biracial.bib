Journal = {Archives of Neurology},
Keywords = {Alzheimer disease, apolipoprotein E, Blacks, Whites, Alzheimer's Disease, Apolipoproteins, Epidemiology, Proteins, Racial and Ethnic Differences, Blacks, Whites, Apolipoprotein E},
Number = {2},
