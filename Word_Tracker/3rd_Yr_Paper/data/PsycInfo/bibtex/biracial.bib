}


@article{2011-26450-00620110301,
Abstract = {The main objective of the current study was to examine racial identity development in a biracial athlete. The variant approach of identity development suggests that individuals of more than one racial background are confronted with a unique set of developmental tasks in order to achieve racial identity. The sport environment may offer a unique context through which racial identity may be developed. A qualitative case study analysis was used to assess the relevance of the variant approach in explaining identity development in a bi-racial athlete. Results revealed partial support for the stages associated with the variant model. Moreover, racial identity is also discussed in relation to sport performance. Some practical recommendations are made for sport psychology practitioners that may encounter bi-racial athletes in their work. Furthermore, suggestions for future research are provided. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
Author = {Stanley, Christopher T. and  Robbins, Jamie E.},
ISSN = {1612-197X, 1557-251X},
Journal = {International Journal of Sport and Exercise Psychology},
Keywords = {racial identity development, sports, biracial athletes, Antisocial Personality Disorder, Athletes, Ethnic Identity, Interracial Offspring, Sports},
Number = {1},
Pages = {64 - 77},
Title = {Racial identity and sport: The case of a bi-racial athlete.},
URL = {stanleych@wssu.edu},
Volume = {9},
Year = {2011},
}


@article{2011-13536-00120110101,
Abstract = {Reviews the book, Black women in interracial relationships: In search of love and solace by Kellina M. Craig-Henderson (see record [rid]2010-21567-000[/rid]). There is generally a lack of research/literature on African American relationships and even less research on Black female relationships. Black women in interracial relationships: In search of love and solace describes a qualitative study of Black women designed to address the prevalence of interracial relationships with Black women, how the relationships come about, how others (family, friends, strangers, coworkers) perceive and react to the relationship, and characteristics of these women. The book provides unique insights into Black and biracial women’s desire for interracial relationships, the unique challenges that they face in entering relationships, and the strategies that women engage in to cope with reactions from family, friends, and strangers intrigued by the relationship. This book is a must read for both clinicians and researchers of Black women. Researchers will benefit from the examination of identity development, psychological functioning, and relationship choices. The true strength of this book is its theoretical discussion and interpretation of women’s experiences. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Thomas, Anita Jones},
ISSN = {1554-0138},
