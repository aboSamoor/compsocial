Volume = {32},
Year = {2001},
}


@article{2001-03009-00320010101,
Abstract = {Performed serial neuropsychological assessments to detect vascular risk factors for cognitive decline in the Atherosclerosis Risk in Communities cohort, a large biracial, multisite, longitudinal investigation of initially middle-aged individuals. Cognitive assessments were administered to 8,729 white and 2,234 black Ss on 2 occasions separated by 6 yrs. Ss were aged 47–70 yrs at the 1st assessment. The cognitive assessments included the delayed word recall test, a 10-word delayed free recall task in which the learning phase included sentence generation with the study words, the digit symbol subtest (DSS) of the Wechsler Adult Intelligence Scale--Revised (WAIS--R), and the first-letter word fluency (WF) test. In multivariate analyses (controlling for demographic factors), the presence of diabetes at baseline was associated with greater decline in scores on both the DSS and WF test, and the presence of hypertension at baseline was associated with greater decline on the DSS alone. The association of diabetes with cognitive decline persisted when analysis was restricted to the 47–57-yr-old subgroup. Smoking status, carotid intima–media wall thickness, and hyperlipidemia at baseline were not associated with change in cognitive test scores. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Knopman, D. and  Boland, L. L. and  Mosley, T. and  Howard, G. and  Liao, D. and  Szklo, M. and  McGovern, P. and  Folsom, A. R.},
