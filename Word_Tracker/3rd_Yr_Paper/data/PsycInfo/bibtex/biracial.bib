Year = {1999},
}


@inbook{1999-04116-00219990101,
Abstract = {Examines communication between parents and children about racially negative interpersonal encounters. The focus is in episodes of racial derogation for African-American families. Specifically, the investigation considers how African-American parents approach the problem of racial insults and what conditions might account for differences in their family communication about the issue. The primary goal of the study is to generate insights to assist African-American parents in preparing their children to maintain feelings of personal integrity and self-control in the face of these negative experiences. A secondary aim of the analysis is to contribute to understanding the potential effects of parent–child communication about race in all US families. Four nonclinical families, two Black and two White, with children ranging in age between 9 and 14 yrs were interviewed. Interview questions tried to assess the family's media behavior patterns and power, parental roles and authority, multigenerational patterns, and the family's "family myth" (i.e., the most important trait or value that the parents were trying to teach their child). Findings suggest that mass media plays a different role in each family system and that the role is informed by a number of other system patterns. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
