Pages = {18 - 24},
Title = {The relationship between binge eating and weight status on depression, anxiety, and body image among a diverse college sample: A focus on bi/multiracial women.},
URL = {vivezaj@emich.edu},
Volume = {11},
Year = {2010},
}


