Volume = {53},
Year = {2002},
}


@article{2002-15227-00920020601,
Abstract = {Examined the prevalence and correlates (demographic, physical, psychological, and behavioral) of binge eating (BE) in a biracial sample of adolescent males and females. 688 White and 134 African American students in Grades 6-12 (ages 10-19 yrs) completed measures of SES, body mass index (BMI), depression, current-ideal body image discrepancy, eating attitudes, dieting frequency, dietary intake, and activity level. BE prevalence was highest among African American boys relative to the other demographic groups: 26% in African American boys, 17% in African American girls, 19% in White boys, and 18% in White girls. BE rates increased with age for White Ss and decreased with age for African American Ss. Depressive symptoms and consumption of high-fat foods predicted BE status, whereas SES, BMI, eating attitudes, body image discrepancy, dieting, and low activity level failed to add predictive value. The observation of developmental differences in binging between Whites and African Americans corresponds to their physical maturational divergence. These findings underscore the need for population-based surveys that sample binge eating across age, gender, and ethnicity. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
