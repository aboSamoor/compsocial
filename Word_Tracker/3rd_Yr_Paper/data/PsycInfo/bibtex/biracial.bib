Title = {When face and soul collide: Therapeutic concerns with racially ambiguous and nonvisible minority women.},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2004-00284-008&site=ehost-live&scope=site},
Year = {2004},
}


@inbook{1999-04116-00719990101,
Abstract = {The two goals of this chapter are to (1) provide an assessment of the current state of knowledge, attitudes, and course practices of family communication educators with respect to African-American culture and, based on these results, (2) begin to lay a foundation upon which to build strategies to increase family communication educators' competence in providing communication instruction both within and across the racial divide. The authors report the results of a survey of 31 family communication educators' knowledge, attitudes, and course practices concerning aspects of African-American culture and family life. Then, the authors use the results of the survey to lay a foundation upon which to build future instructional strategies and research. More specifically, they offer a selected primer of readings concerning African-American culture and family life of relevance to the study of family communication. Results from the survey suggest that family communication education is primarily taught by White educators, teaching mostly White students, using instructional materials that are perceived to not adequately present a culturally diverse picture of communication in families. However, these educators did acknowledge a need for improvement and recognized their need for help. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (chapter)},
Author = {Socha, Thomas J. and  Beigle, Jennifer and  Socha, Thomas J. and  Diggs, Rhunette C. and  Socha, Thomas J. (Ed) and  Diggs, Rhunette C. (Ed)},
Booktitle = {Communication, race, and family:  Exploring communication in Black, White, and biracial families.},
