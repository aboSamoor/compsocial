Journal = {The Journal of Pediatrics},
Keywords = {leukocytes telomere length, healthy adolescents, Caucasians, African Americans, racial differences, sex differences, adiposity, adipokines, physical activity, Adolescent Development, Chromosomes, Leucocytes, Physical Activity, Body Fat, Blacks, Human Sex Differences, Racial and Ethnic Differences, Whites},
Number = {2},
Pages = {215 - 220},
Title = {Leukocyte telomere length in healthy Caucasian and African-American adolescents: Relationships with race, sex, adiposity, adipokines, and physical activity.},
