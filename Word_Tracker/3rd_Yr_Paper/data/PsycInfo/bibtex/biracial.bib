Volume = {17},
Year = {2003},
}


@article{2003-01843-00120030201,
Abstract = {Few studies compare Alzheimer disease (AD) incidence among black and white subjects. The objective of this study is to estimate incidence and the effect of the apolipoprotein E (APOE) ε4 allele in these races. A population-based study of disease incidence using a random, stratified sample was done. The study was done in a geographically defined community of 3 adjacent neighborhoods in Chicago, Ill. A total of 6158 persons participated; 4.1 years later, persons initially free of AD were sampled for clinical evaluation for disease incidence. The effect of the APOE ε4 allele on the risk of AD differed strongly for black and white subjects. Among white subjects, the presence of the APOE ε4 allele was associated with a 2.73-fold increase in risk while among black subjects there was no increase in risk. Black race was associated with a nonsignificantly increased risk of AD with an odds ratio of 1.84 if APOE and its interaction with race are considered, and an odds ratio of 1.28 if they are not. The incidence of AD was 1.45% per year among persons 65 to 74 years old, 4.73% among those 75 to 84 years old, and 9.11% among those 85 years and older. Apolipoprotein E ε4 led to increased risk of AD among white subjects but not black subjects. (PsycINFO Database Record (c) 2014 APA, all rights reserved)},
Author = {Evans, Denis A. and  Bennett, David A. and  Wilson, Robert S. and  Bienias, Julia L. and  Morris, Martha Clare and  Scherr, Paul A. and  Hebert, Liesi E. and  Aggarwal, Neelum and  Beckett, Laurel A. and  Joglekar, Rajiv and  Berry-Kravis, Elizabeth and  Schneider, Julie},
