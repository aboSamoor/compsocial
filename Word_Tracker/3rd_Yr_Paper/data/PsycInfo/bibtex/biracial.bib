Number = {5},
Pages = {M288 - M292},
Title = {The prevalence of dizziness and its association with functional disability in a biracial community population.},
Volume = {55A},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2000-03844-005&site=ehost-live&scope=site},
