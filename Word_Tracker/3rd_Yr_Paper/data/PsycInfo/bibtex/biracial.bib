Keywords = {Chicago Health and Aging Project, risk factors, project design, chronic health problems, older adults, Alzheimers disease, biracial neighborhood, Geriatrics, Health, Risk Factors, Alzheimer's Disease, Racial and Ethnic Differences, Screening},
Number = {5},
