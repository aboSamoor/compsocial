Title = {Negotiating Racial Identity: Biracial Women and Interactional Validation.},
URL = {rockquem@bc.edu},
Volume = {27},
Year = {2004},
}


@book{2004-00284-00020040101,
Abstract = {The papers in this volume represent different levels of insight into the experience of being a biracial woman. Some authors present experiences that are unique to biracial women, while others focus on areas of experience that are common among both biracial women and men. Some are very personal/intimate expressions of the complexity of finding a biracial identity without a supportive community. Each provides a building block to the construction of theory and practice in work with biracial women. We thought it was important to include diverse perspectives on a very complex set of issues related to biracial women. We invite readers to engage in both internal and interactive dialogues about the issues raised in these papers, taking no conclusion at face value, but examining all with a critical and integrative eye. In the following overview, we will point out areas of contrast and agreement in perspective among some of the authors and we encourage the reader to do the same. We hope that by doing so, new questions will emerge that can be researched and that will contribute to our greater understanding of biracial women and the ways that gender affects their experiences. (PsycINFO Database Record (c) 2013 APA, all rights reserved). (introduction)},
Author = {Gillem, Angela R. and  Thompson, Cathy A. and  Gillem, Angela R. and  Thompson, Cathy A.},
