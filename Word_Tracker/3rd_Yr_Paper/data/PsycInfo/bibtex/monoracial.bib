Pages = {41 - 56},
Publisher = {Emerald Group Publishing},
Series = {Research in consumer behavior; Vol 15; ISSN: 0885-2111 (Print)},
Title = {Negotiating cultural ambiguity: A phenomenological study of multiracial identity and consumption.},
