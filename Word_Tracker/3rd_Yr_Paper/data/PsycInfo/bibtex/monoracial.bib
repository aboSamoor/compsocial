@article{2004-10574-00120040101,
Abstract = {Discusses biracial women in therapy. The existence of racially mixed persons in America challenges long-held notions about the biological, moral, and social meaning of race. In order to be effective in the therapeutic setting, therapists and counselors need to be aware of the socio-cultural factors encountered by this population and develop their interventions accordingly. The culturally competent psychologist needs to work from a framework that is not only sensitive to issues regarding minority identity development, but also incorporates an understanding of how biracial identity development differs from monoracial development. It is time to move beyond linear models of identity development that only examine how one deals with racial oppression or ethnic identification to a multidimensional framework that conceptualizes biracial women as racial/ethnic beings, gendered beings, sexual beings, mothers, daughters, partners, and friends. Therapists must be open to the multitude of ways their clients might choose to resolve their biracial identity and be able to facilitate exploration and movement toward goals that are consistent with each client's definition of what is healthy for her. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
Author = {Gillem, Angela R. and  Thompson, Cathy A.},
ISSN = {0270-3149, 1541-0315},
Journal = {Women & Therapy},
Keywords = {biracial identity development, socio-cultural factors, ethnic identification, racial oppression, gender differences, Ethnic Identity, Self Concept, Sociocultural Factors, Therapeutic Processes, Human Females},
Number = {1-2},
Pages = {1 - 18},
Title = {Introduction: Biracial Women in Therapy: Between the Rock of Gender and the Hard Place of Race.},
Volume = {27},
URL = {https://libproxy.cc.stonybrook.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2004-10574-001&site=ehost-live&scope=site},
Year = {2004},
}


