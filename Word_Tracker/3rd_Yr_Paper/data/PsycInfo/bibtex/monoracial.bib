}


@article{2005-13662-01120051101,
Abstract = {We discuss the challenges of small-sub sample sizes that family scholars often encounter when studying nontraditional or less common family types. We begin by identifying the general difficulties of using existing data in this line of research and then discuss potential solutions that may help researchers to avoid these problems. Using data from the Early Childhood Longitudinal Study, we use the example of monoracial White, monoracial Asian, and Asian/White interracial families to illustrate these problems and available, albeit imperfect, solutions. (PsycINFO Database Record (c) 2012 APA, all rights reserved). (journal abstract)},
